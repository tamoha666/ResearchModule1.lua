-- –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò

function ApplySpeedHack()
    local character = Player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = Settings.WalkSpeed
            humanoid.JumpPower = Settings.JumpPower
        end
    end
end

function GetFieldPosition()
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –∫—ç—à –µ—Å–ª–∏ –µ—Å—Ç—å
    if Cache.FieldPosition then
        return Cache.FieldPosition
    end
    
    -- –ò—â–µ–º —Ç–æ—á–Ω–æ–µ –ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø–æ–ª—è
    local targetField = nil
    
    -- –°–Ω–∞—á–∞–ª–∞ –∏—â–µ–º –ø–æ —Ç–æ—á–Ω–æ–º—É –∏–º–µ–Ω–∏
    targetField = workspace:FindFirstChild(Settings.CurrentField)
    
    -- –ï—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏, –∏—â–µ–º –≤ FlowerZones
    if not targetField then
        local flowerZones = workspace:FindFirstChild("FlowerZones")
        if flowerZones then
            for _, zone in pairs(flowerZones:GetChildren()) do
                if zone.Name:find(Settings.CurrentField) then
                    targetField = zone
                    break
                end
            end
        end
    end
    
    -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ –ø–æ–ª–µ
    if targetField then
        if targetField:IsA("BasePart") then
            -- –î–ª—è BasePart –±–µ—Ä—ë–º Position
            Cache.FieldPosition = targetField.Position + Vector3.new(0, 5, 0)
        elseif targetField:IsA("Model") then
            -- –î–ª—è Model –∏—â–µ–º PrimaryPart
            if targetField.PrimaryPart then
                Cache.FieldPosition = targetField.PrimaryPart.Position + Vector3.new(0, 5, 0)
            else
                -- –ï—Å–ª–∏ –Ω–µ—Ç PrimaryPart, –±–µ—Ä—ë–º –ø–æ–∑–∏—Ü–∏—é –ø–µ—Ä–≤–æ–≥–æ BasePart
                for _, child in pairs(targetField:GetChildren()) do
                    if child:IsA("BasePart") then
                        Cache.FieldPosition = child.Position + Vector3.new(0, 5, 0)
                        break
                    end
                end
            end
        end
    end
    
    -- –ï—Å–ª–∏ –≤—Å—ë –µ—â—ë –Ω–µ –Ω–∞—à–ª–∏, –∏—Å–ø–æ–ª—å–∑—É–µ–º –¥–µ—Ñ–æ–ª—Ç–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã
    if not Cache.FieldPosition then
        local defaults = {
            ["Sunflower Field"] = Vector3.new(0, 10, 0),
            ["Pineapple Patch"] = Vector3.new(100, 10, 0),
            ["Bamboo Field"] = Vector3.new(-100, 10, 0),
            ["Mushroom Field"] = Vector3.new(0, 10, 100)
        }
        Cache.FieldPosition = defaults[Settings.CurrentField] or Vector3.new(0, 10, 0)
    end
    
    return Cache.FieldPosition
end

function FindTokens()
    -- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ —Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ —Ç–æ–∫–µ–Ω–æ–≤
    if tick() - Cache.LastScan < Cache.ScanInterval then
        return Cache.Tokens
    end
    
    Cache.LastScan = tick()
    Cache.Tokens = {}
    
    local character = Player.Character
    if not character then return Cache.Tokens end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return Cache.Tokens end
    
    -- –°–∫–∞–Ω–∏—Ä—É–µ–º —Ç–æ–ª—å–∫–æ –±–ª–∏–∂–∞–π—à–∏–µ –æ–±—ä–µ–∫—Ç—ã (—Ä–∞–¥–∏—É—Å 100)
    for _, obj in pairs(workspace:GetChildren()) do
        if obj:IsA("BasePart") and (obj.Name:find("Orb") or obj.Name == "Orb") then
            if (root.Position - obj.Position).Magnitude < 100 then
                table.insert(Cache.Tokens, obj)
            end
        end
    end
    
    return Cache.Tokens
end

function GetNearestToken()
    local tokens = FindTokens()
    local character = Player.Character
    if not character then return nil, 999 end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return nil, 999 end
    
    local nearest = nil
    local nearestDist = 50 -- –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è —Å–±–æ—Ä–∞
    
    for _, token in pairs(tokens) do
        if token and token.Parent then
            local dist = (root.Position - token.Position).Magnitude
            if dist < nearestDist then
                nearest = token
                nearestDist = dist
            end
        end
    end
    
    return nearest, nearestDist
end

function CollectToken(token)
    if not token then return false end
    
    -- –ó–∞—â–∏—Ç–∞ –æ—Ç —Å–ª–∏—à–∫–æ–º —á–∞—Å—Ç–æ–≥–æ —Å–±–æ—Ä–∞
    if tick() - Cache.LastTokenCollect < 0.5 then
        return false
    end
    
    Cache.LastTokenCollect = tick()
    
    local success = false
    
    -- –ú–µ—Ç–æ–¥ 1: TouchInterest (—Å–∞–º—ã–π –Ω–∞–¥—ë–∂–Ω—ã–π)
    pcall(function()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            
            -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è —Ç–æ—á–Ω–æ –Ω–∞–¥ —Ç–æ–∫–µ–Ω–æ–º
            root.CFrame = CFrame.new(token.Position + Vector3.new(0, 2, 0))
            wait(0.05)
            
            -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º TouchInterest
            firetouchinterest(root, token, 0)
            firetouchinterest(root, token, 1)
            
            success = true
        end
    end)
    
    return success
end

function CollectPollen()
    -- –ú–µ—Ç–æ–¥ —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã –≤ Bee Swarm Simulator
    pcall(function()
        local events = game:GetService("ReplicatedStorage"):FindFirstChild("Events")
        if events then
            local playerClick = events:FindFirstChild("PlayerClick")
            if playerClick and playerClick:IsA("RemoteEvent") then
                local character = Player.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    playerClick:FireServer(character.HumanoidRootPart.Position, "Collect")
                end
            end
        end
    end)
end

function StartAutoFarm()
    spawn(function()
        local step = 0
        
        while Settings.AutoFarm do
            -- –û–ü–¢–ò–ú–ò–ó–ê–¶–ò–Ø: –ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É –∏—Ç–µ—Ä–∞—Ü–∏—è–º–∏
            wait(Settings.ClickDelay)
            
            local character = Player.Character
            if not character then 
                wait(1)
                continue 
            end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local root = character:FindFirstChild("HumanoidRootPart")
            if not humanoid or not root then 
                wait(0.5)
                continue 
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 1: –¢–æ–∫–µ–Ω—ã
            if Settings.AutoTokens and Settings.TokenPriority then
                local token, distance = GetNearestToken()
                
                if token and distance < 50 then
                    -- –ò–¥—ë–º –∫ —Ç–æ–∫–µ–Ω—É
                    humanoid:MoveTo(token.Position)
                    
                    -- –ñ–¥—ë–º –ø—Ä–∏–±–ª–∏–∂–µ–Ω–∏—è –∏ —Å–æ–±–∏—Ä–∞–µ–º
                    local attempts = 0
                    while attempts < 5 and Settings.AutoFarm and Settings.AutoTokens do
                        wait(0.2)
                        attempts = attempts + 1
                        
                        if (root.Position - token.Position).Magnitude < 15 then
                            CollectToken(token)
                            wait(0.3)
                            break
                        end
                    end
                    
                    -- –ü–æ—Å–ª–µ —Å–±–æ—Ä–∞ —Ç–æ–∫–µ–Ω–∞ –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º
                    wait(0.2)
                end
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 2: –§–∞—Ä–º –ø—ã–ª—å—Ü—ã
            local fieldCenter = GetFieldPosition()
            
            -- –í—ã—á–∏—Å–ª—è–µ–º —Ü–µ–ª–µ–≤—É—é –ø–æ–∑–∏—Ü–∏—é –Ω–∞ –ø–æ–ª–µ
            local targetPos
            if Settings.WalkMode == "Circle" then
                local angle = step * 0.1
                targetPos = fieldCenter + Vector3.new(
                    math.cos(angle) * Settings.FarmRadius,
                    0,
                    math.sin(angle) * Settings.FarmRadius
                )
                step = step + 1
                
            elseif Settings.WalkMode == "Square" then
                local side = math.floor(step / 10) % 4
                local pos = (step % 40) - 20
                
                if side == 0 then
                    targetPos = fieldCenter + Vector3.new(pos, 0, -Settings.FarmRadius)
                elseif side == 1 then
                    targetPos = fieldCenter + Vector3.new(Settings.FarmRadius, 0, pos)
                elseif side == 2 then
                    targetPos = fieldCenter + Vector3.new(-pos, 0, Settings.FarmRadius)
                else
                    targetPos = fieldCenter + Vector3.new(-Settings.FarmRadius, 0, -pos)
                end
                step = step + 1
                
            else -- Random
                targetPos = fieldCenter + Vector3.new(
                    math.random(-Settings.FarmRadius, Settings.FarmRadius),
                    0,
                    math.random(-Settings.FarmRadius, Settings.FarmRadius)
                )
            end
            
            -- –î–≤–∏–≥–∞–µ–º—Å—è –∫ —Ü–µ–ª–∏
            humanoid:MoveTo(targetPos)
            
            -- –°–æ–±–∏—Ä–∞–µ–º –ø—ã–ª—å—Ü—É
            CollectPollen()
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 3: –¢–æ–∫–µ–Ω—ã —Ä—è–¥–æ–º (–µ—Å–ª–∏ –Ω–µ –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ)
            if Settings.AutoTokens and not Settings.TokenPriority then
                local token, distance = GetNearestToken()
                if token and distance < 20 then
                    CollectToken(token)
                end
            end
        end
    end)
end

function TeleportToField(fieldName)
    local fieldPos = GetFieldPosition()
    local character = Player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(fieldPos + Vector3.new(0, 3, 0))
        print("[–¢–ï–õ–ï–ü–û–†–¢] –ü–µ—Ä–µ–º–µ—â—ë–Ω –Ω–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function ShowStats()
    pcall(function()
        local stats = "\nüìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê:\n"
        
        -- –¢–æ–∫–µ–Ω—ã
        local tokens = FindTokens()
        stats = stats .. "–¢–æ–∫–µ–Ω–æ–≤ Orb —Ä—è–¥–æ–º: " .. #tokens .. "\n"
        
        -- –ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω
        local nearest, dist = GetNearestToken()
        if nearest then
            stats = stats .. "–ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω: " .. math.floor(dist) .. " studs\n"
        end
        
        -- –ü—ã–ª—å—Ü–∞
        if Player:FindFirstChild("PlayerStats") then
            local ps = Player.PlayerStats
            if ps:FindFirstChild("Pollen") then
                stats = stats .. "–ü—ã–ª—å—Ü–∞: " .. ps.Pollen.Value .. "\n"
            end
            if ps:FindFirstChild("Honey") then
                stats = stats .. "–ú—ë–¥: " .. ps.Honey.Value .. "\n"
            end
        end
        
        print(stats)
    end)
end

