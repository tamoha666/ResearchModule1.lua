local farmToggle = CreateToggle("üåª –ê–í–¢–û–§–ê–†–ú –ü–´–õ–¨–¶–´", currentY, false, function(state)
    Settings.AutoFarm = state
    if state then
        print("[AUTOFARM] –ê–≤—Ç–æ—Ñ–∞—Ä–º –∑–∞–ø—É—â–µ–Ω –Ω–∞ –ø–æ–ª–µ: " .. Settings.CurrentField)
        StartAutoFarm()
    else
        print("[AUTOFARM] –ê–≤—Ç–æ—Ñ–∞—Ä–º –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)
currentY = currentY + 50

-- –ê–≤—Ç–æ—Å–±–æ—Ä —Ç–æ–∫–µ–Ω–æ–≤ (–ø–æ–∫–∞ –Ω–µ–∞–∫—Ç–∏–≤–µ–Ω)
local tokenToggle = CreateToggle("üåü –ê–í–¢–û–°–ë–û–† –¢–û–ö–ï–ù–û–í (–°–ö–û–†–û)", currentY, false, function(state)
    Settings.AutoTokens = state
    if state then
        print("[TOKENS] –ê–≤—Ç–æ—Å–±–æ—Ä —Ç–æ–∫–µ–Ω–æ–≤ –±—É–¥–µ—Ç –¥–æ–±–∞–≤–ª–µ–Ω –ø–æ—Å–ª–µ –¥–∏–∞–≥–Ω–æ—Å—Ç–∏–∫–∏")
    end
end)
currentY = currentY + 50

-- –í—ã–±–æ—Ä –ø–æ–ª—è
CreateButton("üåç –ü–æ–ª–µ: " .. Settings.CurrentField, currentY, function()
    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field"}
    local currentIndex = 1
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            currentIndex = i
            break
        end
    end
    local nextIndex = (currentIndex % #fields) + 1
    Settings.CurrentField = fields[nextIndex]
    print("[FIELD] –í—ã–±—Ä–∞–Ω–æ –ø–æ–ª–µ: " .. Settings.CurrentField)
end, Color3.fromRGB(100, 200, 100))
currentY = currentY + 50

-- –†–µ–∂–∏–º —Ö–æ–¥—å–±—ã
CreateButton("üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode, currentY, function()
    local modes = {"Random", "Circle", "Square", "ZigZag"}
    local currentIndex = 1
    for i, mode in ipairs(modes) do
        if mode == Settings.WalkMode then
            currentIndex = i
            break
        end
    end
    local nextIndex = (currentIndex % #modes) + 1
    Settings.WalkMode = modes[nextIndex]
    print("[WALKMODE] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω —Ä–µ–∂–∏–º: " .. Settings.WalkMode)
end)
currentY = currentY + 50

-- –î–∏–∞–≥–Ω–æ—Å—Ç–∏–∫–∞
CreateButton("üîç –î–ò–ê–ì–ù–û–°–¢–ò–ö–ê –¢–û–ö–ï–ù–û–í", currentY, function()
    print("\n=== –ó–ê–ü–£–°–ö –î–ò–ê–ì–ù–û–°–¢–ò–ö–ò ===")
    print("1. –ü–µ—Ä–µ–π–¥–∏ –Ω–∞ –ø–æ–ª–µ —Å —Ç–æ–∫–µ–Ω–∞–º–∏")
    print("2. –ü–æ–¥–æ–∂–¥–∏ –ø–æ–∫–∞ –ø—á—ë–ª—ã –≤—ã–±—Ä–æ—Å—è—Ç —Ç–æ–∫–µ–Ω—ã")
    print("3. –ü–æ—Å–º–æ—Ç—Ä–∏ –≤ –∫–æ–Ω—Å–æ–ª—å –∏–Ω–∂–µ–∫—Ç–æ—Ä–∞")
    
    -- –°–∫–∞–Ω–∏—Ä—É–µ–º –æ–±—ä–µ–∫—Ç—ã –≤–æ–∫—Ä—É–≥
    local objects = workspace:GetDescendants()
    local tokenCount = 0
    
    for _, obj in pairs(objects) do
        if obj:IsA("BasePart") or obj:IsA("MeshPart") then
            local name = obj.Name:lower()
            if name:find("token") or name:find("boost") or name:find("orb") then
                tokenCount = tokenCount + 1
                print(string.format("–¢–æ–∫–µ–Ω #%d: %s (%s)", tokenCount, obj.Name, obj.ClassName))
            end
        end
    end
    
    print("–ù–∞–π–¥–µ–Ω–æ –æ–±—ä–µ–∫—Ç–æ–≤-–∫–∞–Ω–¥–∏–¥–∞—Ç–æ–≤: " .. tokenCount)
    print("–ï—Å–ª–∏ 0 - –ø–æ–¥–æ–∂–¥–∏ –ø–æ–∫–∞ –ø—á—ë–ª—ã –≤—ã–±—Ä–æ—Å—è—Ç —Ç–æ–∫–µ–Ω—ã")
end, Color3.fromRGB(255, 165, 0))
currentY = currentY + 50

-- –¢–µ–ª–µ–ø–æ—Ä—Ç –Ω–∞ –ø–æ–ª–µ
CreateButton("üì° –¢–ï–õ–ï–ü–û–†–¢ –ù–ê –ü–û–õ–ï", currentY, function()
    TeleportToField(Settings.CurrentField)
end, Color3.fromRGB(160, 100, 255))
currentY = currentY + 50

-- –û—Ç–ª–∞–¥–∫–∞
CreateButton("üêõ –†–ï–ñ–ò–ú –û–¢–õ–ê–î–ö–ò", currentY, function()
