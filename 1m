-- –í—Å—Ç–∞–≤—å –≤ –∏–Ω–∂–µ–∫—Ç–æ—Ä: loadstring(game:HttpGet('https://raw.githubusercontent.com/tamoha666/ResearchModule1.lua/refs/heads/main/1m'))()

local Player = game:GetService("Players").LocalPlayer
local RunService = game:GetService("RunService")

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏
print("=== BEE SWARM FARMER –ó–ê–ì–†–£–ñ–ê–ï–¢–°–Ø ===")

-- –û—Å–Ω–æ–≤–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
local Settings = {
    WalkSpeed = 50,
    JumpPower = 50,
    AutoFarm = false,
    AutoTokens = false,
    CurrentField = "Sunflower Field",
    WalkMode = "Random",
    TokenPriority = true
}

-- –°–æ–∑–¥–∞—ë–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "BeeFarmGUI"
ScreenGui.Parent = game.CoreGui

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 300, 0, 400)
MainFrame.Position = UDim2.new(0.05, 0, 0.1, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

-- –ó–∞–≥–æ–ª–æ–≤–æ–∫
local Title = Instance.new("TextLabel")
Title.Text = "üêù BEE FARMER v7"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
Title.TextColor3 = Color3.fromRGB(255, 215, 0)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 16
Title.Parent = MainFrame

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –ø—Ä–æ—Å—Ç–æ–π –∫–Ω–æ–ø–∫–∏
function CreateButton(text, yPos, callback)
    local button = Instance.new("TextButton")
    button.Text = text
    button.Size = UDim2.new(0.9, 0, 0, 35)
    button.Position = UDim2.new(0.05, 0, 0, yPos)
    button.BackgroundColor3 = Color3.fromRGB(60, 60, 90)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.Gotham
    button.TextSize = 14
    button.Parent = MainFrame
    
    button.MouseButton1Click:Connect(function()
        if callback then callback(button) end
    end)
    
    return button
end

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è
function CreateToggle(text, yPos, initialState, callback)
    local toggleFrame = Instance.new("Frame")
    toggleFrame.Size = UDim2.new(0.9, 0, 0, 40)
    toggleFrame.Position = UDim2.new(0.05, 0, 0, yPos)
    toggleFrame.BackgroundTransparency = 1
    toggleFrame.Parent = MainFrame
    
    local toggleText = Instance.new("TextLabel")
    toggleText.Text = text
    toggleText.Size = UDim2.new(0.7, 0, 1, 0)
    toggleText.BackgroundTransparency = 1
    toggleText.TextColor3 = Color3.fromRGB(240, 240, 255)
    toggleText.Font = Enum.Font.Gotham
    toggleText.TextSize = 14
    toggleText.TextXAlignment = Enum.TextXAlignment.Left
    toggleText.Parent = toggleFrame
    
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 50, 0, 25)
    toggleButton.Position = UDim2.new(1, -55, 0.5, -12.5)
    toggleButton.BackgroundColor3 = initialState and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
    toggleButton.Text = initialState and "–í–ö–õ" or "–í–´–ö–õ"
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.TextSize = 12
    toggleButton.Parent = toggleFrame
    
    toggleButton.MouseButton1Click:Connect(function()
        local newState = not (toggleButton.Text == "–í–ö–õ")
        toggleButton.Text = newState and "–í–ö–õ" or "–í–´–ö–õ"
        toggleButton.BackgroundColor3 = newState and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
        
        if callback then
            callback(newState)
        end
    end)
    
    return toggleButton
end

-- –°–æ–∑–¥–∞—ë–º —ç–ª–µ–º–µ–Ω—Ç—ã –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
local currentY = 45

-- SpeedHack –∫–Ω–æ–ø–∫–∏
local speedButton = CreateButton("‚ö° –°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.WalkSpeed, currentY, function(btn)
    Settings.WalkSpeed = Settings.WalkSpeed + 10
    if Settings.WalkSpeed > 200 then Settings.WalkSpeed = 16 end
    ApplySpeedHack()
    btn.Text = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.WalkSpeed
    print("[SPEED] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ: " .. Settings.WalkSpeed)
end)
currentY = currentY + 40

local jumpButton = CreateButton("ü¶ò –ü—Ä—ã–∂–æ–∫: " .. Settings.JumpPower, currentY, function(btn)
    Settings.JumpPower = Settings.JumpPower + 10
    if Settings.JumpPower > 200 then Settings.JumpPower = 50 end
    ApplySpeedHack()
    btn.Text = "ü¶ò –ü—Ä—ã–∂–æ–∫: " .. Settings.JumpPower
    print("[JUMP] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ: " .. Settings.JumpPower)
end)
currentY = currentY + 40

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª–∏
local farmToggle = CreateToggle("üåª –ê–≤—Ç–æ—Ñ–∞—Ä–º –ø—ã–ª—å—Ü—ã", currentY, false, function(state)
    Settings.AutoFarm = state
    if state then
        print("[–ê–í–¢–û–§–ê–†–ú] –ó–∞–ø—É—â–µ–Ω")
        StartAutoFarm()
    else
        print("[–ê–í–¢–û–§–ê–†–ú] –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)
currentY = currentY + 40

local tokenToggle = CreateToggle("üåü –°–±–æ—Ä —Ç–æ–∫–µ–Ω–æ–≤ Orb", currentY, false, function(state)
    Settings.AutoTokens = state
    print("[–¢–û–ö–ï–ù–´] " .. (state and "–ó–∞–ø—É—â–µ–Ω" or "–û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω"))
end)
currentY = currentY + 40

local priorityToggle = CreateToggle("üéØ –¢–æ–∫–µ–Ω—ã –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ", currentY, true, function(state)
    Settings.TokenPriority = state
    print("[–ü–†–ò–û–†–ò–¢–ï–¢] " .. (state and "–í–ö–õ" or "–í–´–ö–õ"))
end)
currentY = currentY + 40

-- –í—ã–±–æ—Ä –ø–æ–ª—è
local fieldButton = CreateButton("üåç –ü–æ–ª–µ: " .. Settings.CurrentField, currentY, function(btn)
    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field"}
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            Settings.CurrentField = fields[(i % #fields) + 1]
            break
        end
    end
    btn.Text = "üåç –ü–æ–ª–µ: " .. Settings.CurrentField
    print("[–ü–û–õ–ï] –í—ã–±—Ä–∞–Ω–æ: " .. Settings.CurrentField)
end)
currentY = currentY + 40

-- –í—ã–±–æ—Ä —Ä–µ–∂–∏–º–∞
local modeButton = CreateButton("üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode, currentY, function(btn)
    local modes = {"Random", "Circle"}
    for i, mode in ipairs(modes) do
        if mode == Settings.WalkMode then
            Settings.WalkMode = modes[(i % #modes) + 1]
            break
        end
    end
    btn.Text = "üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode
    print("[–†–ï–ñ–ò–ú] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: " .. Settings.WalkMode)
end)
currentY = currentY + 40

-- –£—Ç–∏–ª–∏—Ç—ã
CreateButton("üì° –¢–µ–ª–µ–ø–æ—Ä—Ç –Ω–∞ –ø–æ–ª–µ", currentY, function()
    TeleportToField(Settings.CurrentField)
end)
currentY = currentY + 40

CreateButton("üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞", currentY, function()
    ShowStats()
end)
currentY = currentY + 40

CreateButton("‚ùå –ó–∞–∫—Ä—ã—Ç—å", currentY, function()
    ScreenGui:Destroy()
    print("[–ò–ù–¢–ï–†–§–ï–ô–°] –ó–∞–∫—Ä—ã—Ç")
end)

-- –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò

function ApplySpeedHack()
    local character = Player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = Settings.WalkSpeed
            humanoid.JumpPower = Settings.JumpPower
        end
    end
end

function FindTokens()
    local tokens = {}
    
    -- –ò—â–µ–º —Ç–æ–∫–µ–Ω—ã Orb
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name:find("Orb") then
            table.insert(tokens, obj)
        end
    end
    
    return tokens
end

function FindNearestToken()
    local character = Player.Character
    if not character then return nil end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return nil end
    
    local nearest = nil
    local nearestDist = 50
    
    for _, token in pairs(workspace:GetDescendants()) do
        if token:IsA("BasePart") and token.Name:find("Orb") then
            local dist = (root.Position - token.Position).Magnitude
            if dist < nearestDist then
                nearest = token
                nearestDist = dist
            end
        end
    end
    
    return nearest, nearestDist
end

function CollectToken(token)
    if not token then return false end
    
    local success = false
    
    -- –ú–µ—Ç–æ–¥ 1: TouchInterest
    pcall(function()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            firetouchinterest(root, token, 0)
            firetouchinterest(root, token, 1)
            success = true
        end
    end)
    
    if success then return true end
    
    -- –ú–µ—Ç–æ–¥ 2: ProximityPrompt
    pcall(function()
        if token:FindFirstChildWhichIsA("ProximityPrompt") then
            fireproximityprompt(token:FindFirstChildWhichIsA("ProximityPrompt"))
            success = true
        end
    end)
    
    return success
end

function CollectPollen()
    -- –û—Å–Ω–æ–≤–Ω–æ–π –º–µ—Ç–æ–¥ —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã –≤ Bee Swarm
    pcall(function()
        local events = game:GetService("ReplicatedStorage"):FindFirstChild("Events")
        if events then
            local playerClick = events:FindFirstChild("PlayerClick")
            if playerClick and playerClick:IsA("RemoteEvent") then
                local character = Player.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    playerClick:FireServer(character.HumanoidRootPart.Position, "Collect")
                end
            end
        end
    end)
end

function StartAutoFarm()
    spawn(function()
        local angle = 0
        
        while Settings.AutoFarm do
            wait(0.1) -- –ë–∞–∑–æ–≤–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞
            
            local character = Player.Character
            if not character then 
                wait(1)
                continue 
            end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local root = character:FindFirstChild("HumanoidRootPart")
            if not humanoid or not root then 
                wait(0.5)
                continue 
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 1: –¢–æ–∫–µ–Ω—ã
            if Settings.AutoTokens and Settings.TokenPriority then
                local token, distance = FindNearestToken()
                
                if token and distance < 50 then
                    -- –ë–µ–∂–∏–º –∫ —Ç–æ–∫–µ–Ω—É
                    humanoid:MoveTo(token.Position)
                    wait(0.3)
                    
                    -- –ü—Ä–æ–±—É–µ–º —Å–æ–±—Ä–∞—Ç—å
                    if (root.Position - token.Position).Magnitude < 15 then
                        CollectToken(token)
                        wait(0.2)
                    end
                end
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 2: –§–∞—Ä–º –ø—ã–ª—å—Ü—ã
            local fieldPos = GetFieldCenter()
            
            -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â—É—é —Ç–æ—á–∫—É
            local targetPos
            if Settings.WalkMode == "Circle" then
                targetPos = fieldPos + Vector3.new(
                    math.cos(angle) * 40,
                    0,
                    math.sin(angle) * 40
                )
                angle = angle + 0.15
            else -- Random
                targetPos = fieldPos + Vector3.new(
                    math.random(-40, 40),
                    0,
                    math.random(-40, 40)
                )
            end
            
            -- –î–≤–∏–≥–∞–µ–º—Å—è –∏ —Å–æ–±–∏—Ä–∞–µ–º
            humanoid:MoveTo(targetPos)
            CollectPollen()
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 3: –¢–æ–∫–µ–Ω—ã —Ä—è–¥–æ–º (–µ—Å–ª–∏ –Ω–µ –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ)
            if Settings.AutoTokens and not Settings.TokenPriority then
                local token, distance = FindNearestToken()
                if token and distance < 20 then
                    CollectToken(token)
                end
            end
        end
    end)
end

function GetFieldCenter()
    -- –ü—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ –ø–æ–ª–µ
    for _, obj in pairs(workspace:GetChildren()) do
        if obj.Name:find(Settings.CurrentField) and obj:IsA("BasePart") then
            return obj.Position
        end
    end
    
    -- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    local defaults = {
        ["Sunflower Field"] = Vector3.new(0, 0, 0),
        ["Pineapple Patch"] = Vector3.new(100, 0, 0),
        ["Bamboo Field"] = Vector3.new(-100, 0, 0),
        ["Mushroom Field"] = Vector3.new(0, 0, 100)
    }
    
    return defaults[Settings.CurrentField] or Vector3.new(0, 0, 0)
end

function TeleportToField(fieldName)
    local pos = GetFieldCenter()
    local character = Player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))
        print("[–¢–ï–õ–ï–ü–û–†–¢] –ù–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function ShowStats()
    pcall(function()
        print("\n" .. string.rep("=", 40))
        print("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê:")
        
        -- –¢–æ–∫–µ–Ω—ã
        local tokens = FindTokens()
        print("–¢–æ–∫–µ–Ω–æ–≤ Orb –Ω–∞ –∫–∞—Ä—Ç–µ: " .. #tokens)
        
        -- –ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω
        local nearest, dist = FindNearestToken()
        if nearest then
            print("–ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω: " .. math.floor(dist) .. " studs")
        end
        
        -- –ü—ã–ª—å—Ü–∞
        if Player:FindFirstChild("PlayerStats") then
            local ps = Player.PlayerStats
            if ps:FindFirstChild("Pollen") then
                print("–ü—ã–ª—å—Ü–∞: " .. ps.Pollen.Value)
            end
        end
        
        print(string.rep("=", 40))
    end)
end

-- –ê–≤—Ç–æ–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ SpeedHack –ø—Ä–∏ —Å–ø–∞–≤–Ω–µ
Player.CharacterAdded:Connect(function()
    wait(1)
    ApplySpeedHack()
end)

-- –ü—Ä–∏–º–µ–Ω—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
ApplySpeedHack()

-- –£—Å–ø–µ—à–Ω–∞—è –∑–∞–≥—Ä—É–∑–∫–∞
print("\n" .. string.rep("=", 50))
print("‚úÖ BEE SWARM FARMER v7.0 –£–°–ü–ï–®–ù–û –ó–ê–ì–†–£–ñ–ï–ù!")
print("")
