-- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä
ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, currentY + 10)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–Ω–æ–ø–æ–∫
fieldButton.MouseButton1Click = function()
    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field"}
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            Settings.CurrentField = fields[(i % #fields) + 1]
            break
        end
    end
    fieldButton.Text = "üåç –ü–æ–ª–µ: " .. Settings.CurrentField
    print("[–ü–û–õ–ï] –í—ã–±—Ä–∞–Ω–æ: " .. Settings.CurrentField)
end

modeButton.MouseButton1Click = function()
    local modes = {"Random", "Circle", "Square"}
    for i, mode in ipairs(modes) do
        if mode == Settings.WalkMode then
            Settings.WalkMode = modes[(i % #modes) + 1]
            break
        end
    end
    modeButton.Text = "üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode
    print("[–†–ï–ñ–ò–ú] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: " .. Settings.WalkMode)
end

-- –û–ü–¢–ò–ú–ò–ó–ò–†–û–í–ê–ù–ù–´–ï –§–£–ù–ö–¶–ò–ò

function ApplySpeedHack()
    local character = Player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = Settings.WalkSpeed
            humanoid.JumpPower = Settings.JumpPower
        end
    end
end

function ScanForTokens()
    -- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ —Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ - —Ç–æ–ª—å–∫–æ –µ—Å–ª–∏ –ø—Ä–æ—à–ª–æ –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –≤—Ä–µ–º–µ–Ω–∏
    if tick() - Cache.LastTokenScan < Cache.TokenScanInterval then
        return Cache.Tokens
    end
    
    Cache.LastTokenScan = tick()
    Cache.Tokens = {}
    
    -- –í —Ä–µ–∂–∏–º–µ –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ —Å–∫–∞–Ω–∏—Ä—É–µ–º —Ç–æ–ª—å–∫–æ –±–ª–∏–∂–∞–π—à–∏–µ –æ–±—ä–µ–∫—Ç—ã
    if Settings.PerformanceMode then
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            
            -- –¢–æ–ª—å–∫–æ –æ–±—ä–µ–∫—Ç—ã –≤ —Ä–∞–¥–∏—É—Å–µ 100
            for _, obj in pairs(workspace:GetChildren()) do
                if obj:IsA("BasePart") and (obj.Name:find("Orb") or obj.Name == "Orb") then
                    if (root.Position - obj.Position).Magnitude < 100 then
                        table.insert(Cache.Tokens, obj)
                    end
                end
            end
        end
    else
        -- –ü–æ–ª–Ω–æ–µ —Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("BasePart") and (obj.Name:find("Orb") or obj.Name == "Orb") then
                table.insert(Cache.Tokens, obj)
            end
        end
    end
    
    return Cache.Tokens
end

function GetNearestToken()
    local character = Player.Character
    if not character then return nil end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return nil end
    
    local tokens = ScanForTokens()
    local nearest = nil
    local nearestDist = 50 -- –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–∞
    
    for _, token in pairs(tokens) do
        if token and token.Parent then
            local dist = (root.Position - token.Position).Magnitude
            if dist < nearestDist then
                nearest = token
                nearestDist = dist
            end
        end
    end
    
    return nearest, nearestDist
end

function CollectToken(token)
    if not token then return false end
    
    -- –ü—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –º–µ—Ç–æ–¥—ã
    local success = false
    
    -- –ú–µ—Ç–æ–¥ 1: TouchInterest (—Å–∞–º—ã–π –±—ã—Å—Ç—Ä—ã–π)
    pcall(function()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            
            -- –ë—ã—Å—Ç—Ä–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ç–æ–∫–µ–Ω—É
            root.CFrame = CFrame.new(token.Position + Vector3.new(0, 2, 0))
            
            -- TouchInterest
            firetouchinterest(root, token, 0)
            firetouchinterest(root, token, 1)
            
            success = true
        end
    end)
    
    if success then return true end
    
    -- –ú–µ—Ç–æ–¥ 2: ProximityPrompt
    pcall(function()
        if token:FindFirstChildWhichIsA("ProximityPrompt") then
            fireproximityprompt(token:FindFirstChildWhichIsA("ProximityPrompt"))
            success = true
        end
    end)
    
    return success
end

function CollectPollen()
    -- –í Bee Swarm Simulator –ø—ã–ª—å—Ü–∞ —Å–æ–±–∏—Ä–∞–µ—Ç—Å—è —á–µ—Ä–µ–∑ RemoteEvent PlayerClick
    pcall(function()
        local events = game:GetService("ReplicatedStorage"):FindFirstChild("Events")
        if events then
            local playerClick = events:FindFirstChild("PlayerClick")
            if playerClick and playerClick:IsA("RemoteEvent") then
                local character = Player.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    playerClick:FireServer(character.HumanoidRootPart.Position, "Collect")
                end
            end
        end
    end)
end

function StartAutoFarm()
    spawn(function()
        local step = 0
        local angle = 0
        
        while Settings.AutoFarm do
            -- –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è: –º–µ–Ω—å—à–µ –ø—Ä–æ–≤–µ—Ä–æ–∫
            if Settings.PerformanceMode then
                wait(0.05)
            else
                wait(0.02)
            end
            
            local character = Player.Character
            if not character then 
                wait(1)
                continue 
            end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local root = character:FindFirstChild("HumanoidRootPart")
            if not humanoid or not root then 
                wait(0.5)
                continue 
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 1: –¢–æ–∫–µ–Ω—ã (–µ—Å–ª–∏ –≤–∫–ª—é—á–µ–Ω—ã –∏ –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ)
            if Settings.AutoTokens and Settings.TokenPriority then
                local token, distance = GetNearestToken()
                
                if token and distance < 50 then
                    -- –ò–¥—ë–º –∫ —Ç–æ–∫–µ–Ω—É
                    humanoid:MoveTo(token.Position)
                    
                    -- –ñ–¥—ë–º –ø–æ–∫–∞ –ø–æ–¥–æ–π–¥—ë–º –±–ª–∏–∂–µ
                    for i = 1, 10 do
                        if not Settings.AutoFarm then break end
                        wait(0.1)
                        
                        -- –ü—Ä–æ–±—É–µ–º —Å–æ–±—Ä–∞—Ç—å —Ç–æ–∫–µ–Ω
                        if (root.Position - token.Position).Magnitude < 10 then
                            CollectToken(token)
                            wait(0.1)
                            break
                        end
                    end
                    
                    -- –ü–æ—Å–ª–µ —Å–±–æ—Ä–∞ —Ç–æ–∫–µ–Ω–∞ –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º —Ñ–∞—Ä–º
                    wait(0.2)
                end
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 2: –§–∞—Ä–º –ø—ã–ª—å—Ü—ã
            local fieldPos = GetFieldCenter()
            
            -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â—É—é —Ç–æ—á–∫—É
            local targetPos
            if Settings.WalkMode == "Circle" then
                targetPos = fieldPos + Vector3.new(
                    math.cos(angle) * Settings.FarmRadius,
                    0,
                    math.sin(angle) * Settings.FarmRadius
                )
                angle = angle + 0.1
                
            elseif Settings.WalkMode == "Square" then
                local side = math.floor(step / 20) % 4
                local pos = step % (Settings.FarmRadius * 2)
                
                if side == 0 then
                    targetPos = fieldPos + Vector3.new(pos - Settings.FarmRadius, 0, -Settings.FarmRadius)
                elseif side == 1 then
                    targetPos = fieldPos + Vector3.new(Settings.FarmRadius, 0, pos - Settings.FarmRadius)
                elseif side == 2 then
                    targetPos = fieldPos + Vector3.new(Settings.FarmRadius - pos, 0, Settings.FarmRadius)
                else
                    targetPos = fieldPos + Vector3.new(-Settings.FarmRadius, 0, Settings.FarmRadius - pos)
                end
                step = step + 10
                
            else -- Random
                targetPos = fieldPos + Vector3.new(
                    math.random(-Settings.FarmRadius, Settings.FarmRadius),
                    0,
                    math.random(-Settings.FarmRadius, Settings.FarmRadius)
                )
            end
            
            -- –î–≤–∏–≥–∞–µ–º—Å—è
            humanoid:MoveTo(targetPos)
            
            -- –°–æ–±–∏—Ä–∞–µ–º –ø—ã–ª—å—Ü—É (LMB)
            CollectPollen()
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 3: –¢–æ–∫–µ–Ω—ã (–µ—Å–ª–∏ –Ω–µ –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ, –Ω–æ —Ä—è–¥–æ–º)
            if Settings.AutoTokens and not Settings.TokenPriority then
                local token, distance = GetNearestToken()
                if token and distance < 20 then
                    CollectToken(token)
                end
            end
        end
    end)
end

function GetFieldCenter()
    -- –ë—ã—Å—Ç—Ä—ã–π –ø–æ–∏—Å–∫ –ø–æ–ª—è
    if tick() - Cache.LastFieldCheck > 5 then
        Cache.LastFieldCheck = tick()
        Cache.FieldObjects = {}
        
        for _, obj in pairs(workspace:GetChildren()) do
            if obj.Name:find(Settings.CurrentField) then
                Cache.FieldObjects[obj.Name] = obj
            end
        end
    end
    
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –∫—ç—à
    for name, obj in pairs(Cache.FieldObjects) do
        if name:find(Settings.CurrentField) and obj:IsA("BasePart") then
            return obj.Position
        end
    end
    
    -- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    local defaults = {
        ["Sunflower Field"] = Vector3.new(0, 0, 0),
        ["Pineapple Patch"] = Vector3.new(100, 0, 0),
        ["Bamboo Field"] = Vector3.new(-100, 0, 0),
        ["Mushroom Field"] = Vector3.new(0, 0, 100)
    }
    
    return defaults[Settings.CurrentField] or Vector3.new(0, 0, 0)
end

function TeleportToField(fieldName)
    local pos = GetFieldCenter()
    local character = Player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))
        print("[–¢–ï–õ–ï–ü–û–†–¢] –ù–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function ShowStats()
    pcall(function()
        local stats = ""
        
        -- –¢–æ–∫–µ–Ω—ã –≤ —Ä–∞–¥–∏—É—Å–µ
        local tokens = ScanForTokens()
        stats = stats .. "–¢–æ–∫–µ–Ω–æ–≤ —Ä—è–¥–æ–º: " .. #tokens .. "\n"
        
        -- –ü—ã–ª—å—Ü–∞
        if Player:FindFirstChild("PlayerStats") then
            local ps = Player.PlayerStats
            if ps:FindFirstChild("Pollen") then
                stats = stats .. "–ü—ã–ª—å—Ü–∞: " .. ps.Pollen.Value .. "\n"
            end
        end
        
        print("\n" .. string.rep("=", 30))
        print("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê:")
        print(stats)
        print(string.rep("=", 30))
    end)
end

