end, Color3.fromRGB(255, 100, 100))

-- –§–£–ù–ö–¶–ò–ò –ê–í–¢–û–§–ê–†–ú–ê

function StartAutoFarm()
    spawn(function()
        local walkPattern = {
            Random = function(center)
                return center + Vector3.new(
                    math.random(-Settings.FarmRadius, Settings.FarmRadius),
                    0,
                    math.random(-Settings.FarmRadius, Settings.FarmRadius)
                )
            end,
            
            Circle = function(center, angle)
                local radius = Settings.FarmRadius * 0.8
                return center + Vector3.new(
                    math.cos(angle) * radius,
                    0,
                    math.sin(angle) * radius
                )
            end,
            
            Square = function(center, step)
                local side = math.floor(step / 4) % 4
                local pos = step % (Settings.FarmRadius * 2)
                
                if side == 0 then
                    return center + Vector3.new(pos - Settings.FarmRadius, 0, -Settings.FarmRadius)
                elseif side == 1 then
                    return center + Vector3.new(Settings.FarmRadius, 0, pos - Settings.FarmRadius)
                elseif side == 2 then
                    return center + Vector3.new(Settings.FarmRadius - pos, 0, Settings.FarmRadius)
                else
                    return center + Vector3.new(-Settings.FarmRadius, 0, Settings.FarmRadius - pos)
                end
            end
        }
        
        local step = 0
        local angle = 0
        
        while Settings.AutoFarm do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local humanoid = character:FindFirstChild("Humanoid")
                local root = character:FindFirstChild("HumanoidRootPart")
                if not humanoid or not root then return end
                
                -- –ü–æ–ª—É—á–∞–µ–º —Ü–µ–Ω—Ç—Ä —Ç–µ–∫—É—â–µ–≥–æ –ø–æ–ª—è
                local fieldCenter = GetFieldCenter()
                
                -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â—É—é —Ç–æ—á–∫—É –¥–ª—è –¥–≤–∏–∂–µ–Ω–∏—è
                local targetPosition
                if Settings.WalkMode == "Circle" then
                    targetPosition = walkPattern.Circle(fieldCenter, angle)
                    angle = angle + 0.1
                elseif Settings.WalkMode == "Square" then
                    targetPosition = walkPattern.Square(fieldCenter, step)
                    step = step + 5
                else -- Random
                    targetPosition = walkPattern.Random(fieldCenter)
                end
                
                -- –î–≤–∏–≥–∞–µ–º—Å—è –∫ —Ç–æ—á–∫–µ
                humanoid:MoveTo(targetPosition)
                
                -- –î–µ–ª–∞–µ–º –∫–ª–∏–∫–∏ –¥–ª—è —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã
                ClickForPollen()
                
                if Settings.DebugMode then
                    print(string.format("[DEBUG] –î–≤–∏–∂–µ–Ω–∏–µ –∫: %s, –ü–æ–∑–∏—Ü–∏—è: %s", 
                        tostring(targetPosition), tostring(root.Position)))
                end
                
                -- –ñ–¥—ë–º –Ω–µ–º–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º –¥–µ–π—Å—Ç–≤–∏–µ–º
                wait(0.3)
            end)
        end
    end)
end

function ClickForPollen()
    -- –°–∏–º—É–ª–∏—Ä—É–µ–º –∫–ª–∏–∫ LMB –¥–ª—è —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã
    -- –í Bee Swarm Simulator —ç—Ç–æ –º–æ–∂–µ—Ç –±—ã—Ç—å:
    -- 1. –°–æ–±—ã—Ç–∏–µ –≤ ReplicatedStorage
    -- 2. –í—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ —Å–±–æ—Ä–∞
    -- 3. –ü—Ä–æ—Å—Ç–æ –∫–ª–∏–∫ –º—ã—à–∏
    
    pcall(function()
        -- –ü–æ–ø—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –º–µ—Ç–æ–¥—ã
        
        -- –ú–µ—Ç–æ–¥ 1: –ß–µ—Ä–µ–∑ ReplicatedStorage (—Å–∞–º—ã–π –≤–µ—Ä–æ—è—Ç–Ω—ã–π)
        if game:GetService("ReplicatedStorage"):FindFirstChild("Events") then
            local events = game:GetService("ReplicatedStorage").Events
            
            -- –ü–æ–ø—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ —Å–æ–±—ã—Ç–∏–µ —Å–±–æ—Ä–∞
            for _, event in pairs(events:GetChildren()) do
                if event:IsA("RemoteEvent") then
                    if event.Name:find("Click") or event.Name:find("Collect") then
                        event:FireServer("Collect")
                        if Settings.DebugMode then
                            print("[CLICK] –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —Å–æ–±—ã—Ç–∏–µ: " .. event.Name)
                        end
                    end
                end
            end
        end
        
        -- –ú–µ—Ç–æ–¥ 2: –ü—Ä–æ—Å—Ç–æ –∫–ª–∏–∫ (–µ—Å–ª–∏ –∏–≥—Ä–∞ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç Input)
        -- –≠—Ç–æ —Å—Ä–∞–±–æ—Ç–∞–µ—Ç —Ç–æ–ª—å–∫–æ –µ—Å–ª–∏ –∏–Ω–∂–µ–∫—Ç–æ—Ä –º–æ–∂–µ—Ç —Å–∏–º—É–ª–∏—Ä–æ–≤–∞—Ç—å —Ä–µ–∞–ª—å–Ω—ã–π –∫–ª–∏–∫
        if UIS then
            -- –°–æ–∑–¥–∞—ë–º —Ñ–∏–∫—Ç–∏–≤–Ω—ã–π –∫–ª–∏–∫
            -- (–≠—Ç–æ –º–æ–∂–µ—Ç –Ω–µ —Å—Ä–∞–±–æ—Ç–∞—Ç—å –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –∑–∞—â–∏—Ç—ã –∏–≥—Ä—ã)
        end
        
        -- –ú–µ—Ç–æ–¥ 3: –ï—Å–ª–∏ –µ—Å—Ç—å –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç —Å–±–æ—Ä–∞
        local character = Player.Character
        if character then
            local tool = character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Click") then
                tool.Click:Fire()
            end
        end
    end)
    
    wait(Settings.ClickDelay)
end

function GetFieldCenter()
    -- –ü—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ –ø–æ–ª–µ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏
    local field = workspace:FindFirstChild(Settings.CurrentField)
    if field and field:IsA("BasePart") then
        return field.Position
    end
    
    -- –ò—â–µ–º –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é –≤ –ø–æ—Ç–æ–º–∫–∞—Ö
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name:find(Settings.CurrentField) and obj:IsA("BasePart") then
            FieldPositions[Settings.CurrentField] = obj.Position
            return obj.Position
        end
    end
    
    -- –ï—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏, –∏—Å–ø–æ–ª—å–∑—É–µ–º —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—É—é –ø–æ–∑–∏—Ü–∏—é
    return FieldPositions[Settings.CurrentField] or Vector3.new(0, 0, 0)
end

function TeleportToField(fieldName)
    local center = GetFieldCenter()
    local character = Player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(center + Vector3.new(0, 5, 0))
        print("[TELEPORT] –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω –Ω–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function ApplySpeedHack()
    pcall(function()
        local character = Player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = Settings.WalkSpeed
                humanoid.JumpPower = Settings.JumpPower
            end
        end
    end)
end

function ShowStats()
    print("\n" .. string.rep("=", 50))
    print("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê –°–ö–†–ò–ü–¢–ê")
    print("–°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.WalkSpeed)
    print("–ü—Ä—ã–∂–æ–∫: " .. Settings.JumpPower)
    print("–ü–æ–ª–µ: " .. Settings.CurrentField)
    print("–†–µ–∂–∏–º: " .. Settings.WalkMode)
    print("–ê–≤—Ç–æ—Ñ–∞—Ä–º: " .. (Settings.AutoFarm and "–í–ö–õ" or "–í–´–ö–õ"))
    print("–¢–æ–∫–µ–Ω—ã: " .. (Settings.AutoTokens and "–í–ö–õ" or "–í–´–ö–õ"))
    print(string.rep("=", 50))
end

-- –ê–≤—Ç–æ–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ SpeedHack
Player.CharacterAdded:Connect(function()
    wait(1)
    ApplySpeedHack()
end)

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
ApplySpeedHack()

print("\n" .. string.rep("=", 60))
print("üêù BEE SWARM ULTIMATE FARMER v4.0")
print("üë§ –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å: " .. Player.Name)
print("üéÆ –ò–Ω–∂–µ–∫—Ç–æ—Ä: Xeno")
print("")
print("üöÄ –§–£–ù–ö–¶–ò–ò:")
