-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏
print("=== BEE SWARM FARMER –ó–ê–ì–†–£–ñ–ï–ù ===")

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local Settings = {
    WalkSpeed = 50,
    JumpPower = 50,
    AutoFarm = false,
    AutoTokens = false,
    CurrentField = "Sunflower Field",
    WalkMode = "Circle",
    TokenPriority = true,
    FarmRadius = 30,
    ClickDelay = 0.2
}

-- –°–æ–∑–¥–∞—ë–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "BeeFarmFinal"
ScreenGui.Parent = game.CoreGui

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 300, 0, 450)
MainFrame.Position = UDim2.new(0.05, 0, 0.1, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Text = "üêù BEE FARMER v9.0"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
Title.TextColor3 = Color3.fromRGB(255, 215, 0)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 16
Title.Parent = MainFrame

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–∫–∏ —Å –æ–±–Ω–æ–≤–ª—è–µ–º—ã–º —Ç–µ–∫—Å—Ç–æ–º
function CreateButton(text, yPos, color, callback)
    local button = Instance.new("TextButton")
    button.Name = "Btn_" .. text:gsub("%W", ""):sub(1, 10)
    button.Text = text
    button.Size = UDim2.new(0.9, 0, 0, 35)
    button.Position = UDim2.new(0.05, 0, 0, yPos)
    button.BackgroundColor3 = color or Color3.fromRGB(65, 65, 95)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.Gotham
    button.TextSize = 12
    button.Parent = MainFrame
    
    button.MouseButton1Click:Connect(function()
        if callback then callback(button) end
    end)
    
    return button
end

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è
function CreateToggle(text, yPos, initialState, callback)
    local toggleFrame = Instance.new("Frame")
    toggleFrame.Size = UDim2.new(0.9, 0, 0, 35)
    toggleFrame.Position = UDim2.new(0.05, 0, 0, yPos)
    toggleFrame.BackgroundTransparency = 1
    toggleFrame.Parent = MainFrame
    
    local toggleText = Instance.new("TextLabel")
    toggleText.Text = text
    toggleText.Size = UDim2.new(0.7, 0, 1, 0)
    toggleText.BackgroundTransparency = 1
    toggleText.TextColor3 = Color3.fromRGB(220, 220, 255)
    toggleText.Font = Enum.Font.Gotham
    toggleText.TextSize = 12
    toggleText.TextXAlignment = Enum.TextXAlignment.Left
    toggleText.Parent = toggleFrame
    
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 45, 0, 20)
    toggleButton.Position = UDim2.new(1, -45, 0.5, -10)
    toggleButton.BackgroundColor3 = initialState and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(180, 0, 0)
    toggleButton.Text = initialState and "ON" or "OFF"
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.TextSize = 10
    toggleButton.Parent = toggleFrame
    
    toggleButton.MouseButton1Click:Connect(function()
        local newState = not (toggleButton.Text == "ON")
        toggleButton.Text = newState and "ON" or "OFF"
        toggleButton.BackgroundColor3 = newState and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(180, 0, 0)
        
        if callback then
            callback(newState)
        end
    end)
    
    return toggleButton
end

-- SpeedHack –∫–Ω–æ–ø–∫–∏
local currentY = 45

local speedButton = CreateButton("‚ö° –°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.WalkSpeed, currentY, Color3.fromRGB(80, 80, 120), function(btn)
    Settings.WalkSpeed = Settings.WalkSpeed + 10
    if Settings.WalkSpeed > 200 then Settings.WalkSpeed = 16 end
    ApplySpeedHack()
    btn.Text = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.WalkSpeed
end)
currentY = currentY + 40

local jumpButton = CreateButton("ü¶ò –ü—Ä—ã–∂–æ–∫: " .. Settings.JumpPower, currentY, Color3.fromRGB(80, 80, 120), function(btn)
    Settings.JumpPower = Settings.JumpPower + 10
    if Settings.JumpPower > 200 then Settings.JumpPower = 50 end
    ApplySpeedHack()
    btn.Text = "ü¶ò –ü—Ä—ã–∂–æ–∫: " .. Settings.JumpPower
end)
currentY = currentY + 40

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª–∏
local farmToggle = CreateToggle("üåª –ê–≤—Ç–æ—Ñ–∞—Ä–º –ø—ã–ª—å—Ü—ã", currentY, false, function(state)
    Settings.AutoFarm = state
    if state then
        print("[–ê–í–¢–û–§–ê–†–ú] –ó–∞–ø—É—â–µ–Ω - LMB —Å–±–æ—Ä")
        StartAutoFarm()
    else
        print("[–ê–í–¢–û–§–ê–†–ú] –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)
currentY = currentY + 40

local tokenToggle = CreateToggle("üåü –°–±–æ—Ä Red Boost", currentY, false, function(state)
    Settings.AutoTokens = state
    print("[–¢–û–ö–ï–ù–´] " .. (state and "–í–∫–ª—é—á–µ–Ω —Å–±–æ—Ä Red Boost" or "–í—ã–∫–ª—é—á–µ–Ω"))
end)
currentY = currentY + 40

local priorityToggle = CreateToggle("üéØ –¢–æ–∫–µ–Ω—ã –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ", currentY, true, function(state)
    Settings.TokenPriority = state
    print("[–ü–†–ò–û–†–ò–¢–ï–¢] " .. (state and "–í–ö–õ - —Ç–æ–∫–µ–Ω—ã –≤–∞–∂–Ω–µ–µ" or "–í–´–ö–õ - —Ñ–∞—Ä–º –≤–∞–∂–Ω–µ–µ"))
end)
currentY = currentY + 40

-- –í—ã–±–æ—Ä –ø–æ–ª—è (–ò–°–ü–†–ê–í–õ–ï–ù–ê –ö–ù–û–ü–ö–ê)
local fieldButton = CreateButton("üåç –ü–æ–ª–µ: " .. Settings.CurrentField, currentY, Color3.fromRGB(70, 110, 70), function(btn)
    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field"}
    
    -- –ù–∞—Ö–æ–¥–∏–º —Ç–µ–∫—É—â–∏–π –∏–Ω–¥–µ–∫—Å
    local currentIndex = 1
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            currentIndex = i
            break
        end
    end
    
    -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â–µ–µ –ø–æ–ª–µ
    local nextIndex = currentIndex + 1
    if nextIndex > #fields then
        nextIndex = 1
    end
    
    Settings.CurrentField = fields[nextIndex]
    btn.Text = "üåç –ü–æ–ª–µ: " .. Settings.CurrentField
    print("[–ü–û–õ–ï] –í—ã–±—Ä–∞–Ω–æ: " .. Settings.CurrentField)
end)
currentY = currentY + 40

-- –í—ã–±–æ—Ä —Ä–µ–∂–∏–º–∞
local modeButton = CreateButton("üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode, currentY, nil, function(btn)
    local modes = {"Circle", "Square", "Random"}
    
    local currentIndex = 1
    for i, mode in ipairs(modes) do
        if mode == Settings.WalkMode then
            currentIndex = i
            break
        end
    end
    
    local nextIndex = currentIndex + 1
    if nextIndex > #modes then
        nextIndex = 1
    end
    
    Settings.WalkMode = modes[nextIndex]
    btn.Text = "üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode
    print("[–†–ï–ñ–ò–ú] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: " .. Settings.WalkMode)
end)
currentY = currentY + 40

-- –£—Ç–∏–ª–∏—Ç—ã
CreateButton("üì° –¢–µ–ª–µ–ø–æ—Ä—Ç –Ω–∞ –ø–æ–ª–µ", currentY, Color3.fromRGB(80, 80, 140), function()
    TeleportToField(Settings.CurrentField)
end)
currentY = currentY + 40

CreateButton("üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞", currentY, Color3.fromRGB(70, 110, 70), function()
    ShowStats()
end)
currentY = currentY + 40

CreateButton("‚ùå –ó–∞–∫—Ä—ã—Ç—å", currentY, Color3.fromRGB(180, 60, 60), function()
    ScreenGui:Destroy()
    print("[–ò–ù–¢–ï–†–§–ï–ô–°] –ó–∞–∫—Ä—ã—Ç")
end)

-- –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò

function ApplySpeedHack()
    local character = Player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = Settings.WalkSpeed
            humanoid.JumpPower = Settings.JumpPower
        end
    end
end

-- –ò–°–ü–†–ê–í–õ–ï–ù–ù–´–ô LMB –°–ë–û–† –ü–´–õ–¨–¶–´
function CollectPollen()
    -- –í Bee Swarm Simulator –µ—Å—Ç—å –Ω–µ—Å–∫–æ–ª—å–∫–æ —Å–ø–æ—Å–æ–±–æ–≤ —Å–±–æ—Ä–∞
    -- –ü–æ–ø—Ä–æ–±—É–µ–º –≤—Å–µ –º–µ—Ç–æ–¥—ã
    
    local success = false
    
    -- –ú–ï–¢–û–î 1: –ß–µ—Ä–µ–∑ ReplicatedStorage.Events.PlayerClick
    pcall(function()
        local events = game:GetService("ReplicatedStorage"):FindFirstChild("Events")
        if events then
            -- –ü–æ–ø—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –∏–º–µ–Ω–∞ —Å–æ–±—ã—Ç–∏–π
            local eventNames = {"PlayerClick", "Click", "CollectPollen", "Gather"}
            
            for _, eventName in ipairs(eventNames) do
                local event = events:FindFirstChild(eventName)
                if event and event:IsA("RemoteEvent") then
                    local character = Player.Character
                    if character and character:FindFirstChild("HumanoidRootPart") then
                        -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –ø–æ–∑–∏—Ü–∏—é –∏–≥—Ä–æ–∫–∞ –∏ –∫–æ–º–∞–Ω–¥—É —Å–±–æ—Ä–∞
                        event:FireServer(character.HumanoidRootPart.Position, "Collect")
                        success = true
                        break
                    end
                end
            end
        end
    end)
    
    -- –ú–ï–¢–û–î 2: –ï—Å–ª–∏ –µ—Å—Ç—å –∞–∫—Ç–∏–≤–Ω—ã–π –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç (—Ä–µ–¥–∫–æ)
    if not success then
        pcall(function()
            local character = Player.Character
            if character then
                local tool = character:FindFirstChildOfClass("Tool")
                if tool and tool:FindFirstChild("Activate") then
                    tool.Activate:FireServer()
                    success = true
                end
            end
        end)
    end
    
    -- –ú–ï–¢–û–î 3: –ü—Ä–æ—Å—Ç–æ —Å–æ–æ–±—â–∏–º —á—Ç–æ –ø—ã—Ç–∞–µ–º—Å—è —Å–æ–±—Ä–∞—Ç—å
    if not success then
        print("[LMB] –ò—â—É —Å–ø–æ—Å–æ–± —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã...")
    end
    
    return success
end

-- –ü–û–ò–°–ö –¢–û–ö–ï–ù–û–í "Red Boost" (–ò–°–ü–†–ê–í–õ–ï–ù–û)
function FindTokens()
    local tokens = {}
    
    -- –ò—â–µ–º —Ç–æ–∫–µ–Ω—ã –ø–æ –∏–º–µ–Ω–∏ "Red Boost" –∏–ª–∏ —Å–æ–¥–µ—Ä–∂–∞—â–∏–µ "Boost"
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name
            -- –ò—â–µ–º Red Boost –∏–ª–∏ –ª—é–±—ã–µ –±—É—Å—Ç—ã
            if name == "Red Boost" or name:find("Boost") or name:find("Token") then
                table.insert(tokens, obj)
            end
        end
    end
    
    return tokens
end

function FindNearestToken()
    local character = Player.Character
    if not character then return nil, 999 end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return nil, 999 end
    
    local tokens = FindTokens()
    local nearest = nil
    local nearestDist = 50 -- –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–∞
    
    for _, token in pairs(tokens) do
        if token and token.Parent then
            local dist = (root.Position - token.Position).Magnitude
            if dist < nearestDist then
                nearest = token
                nearestDist = dist
            end
        end
    end
    
    return nearest, nearestDist
end

function CollectToken(token)
    if not token then return false end
    
    local success = false
    
    -- –ú–µ—Ç–æ–¥ 1: TouchInterest (—Å–∞–º—ã–π –Ω–∞–¥—ë–∂–Ω—ã–π)
    pcall(function()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            
            -- –ë—ã—Å—Ç—Ä–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ç–æ–∫–µ–Ω—É
            root.CFrame = CFrame.new(token.Position + Vector3.new(0, 2, 0))
            
            -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º TouchInterest
            firetouchinterest(root, token, 0)
            firetouchinterest(root, token, 1)
            
            success = true
            print("[–¢–û–ö–ï–ù] –°–æ–±—Ä–∞–Ω: " .. token.Name)
        end
    end)
    
    if success then return true end
    
    -- –ú–µ—Ç–æ–¥ 2: ProximityPrompt
    pcall(function()
        if token:FindFirstChildWhichIsA("ProximityPrompt") then
            fireproximityprompt(token:FindFirstChildWhichIsA("ProximityPrompt"))
            success = true
        end
    end)
    
    return success
end

function StartAutoFarm()
    spawn(function()
        local step = 0
        
        while Settings.AutoFarm do
            wait(Settings.ClickDelay) -- –ó–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏
            
            local character = Player.Character
            if not character then 
                wait(1)
                continue 
            end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local root = character:FindFirstChild("HumanoidRootPart")
            if not humanoid or not root then 
                wait(0.5)
                continue 
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 1: –¢–æ–∫–µ–Ω—ã Red Boost
            if Settings.AutoTokens and Settings.TokenPriority then
                local token, distance = FindNearestToken()
                
                if token and distance < 50 then
                    -- –ë–µ–∂–∏–º –∫ —Ç–æ–∫–µ–Ω—É
                    humanoid:MoveTo(token.Position)
                    
                    -- –ñ–¥—ë–º –ø–æ–∫–∞ –ø–æ–¥–æ–π–¥—ë–º –±–ª–∏–∑–∫–æ –∏ —Å–æ–±–∏—Ä–∞–µ–º
                    for i = 1, 5 do
                        wait(0.2)
                        if not Settings.AutoFarm then break end
                        
                        if (root.Position - token.Position).Magnitude < 15 then
                            CollectToken(token)
                            wait(0.3)
                            break
                        end
                    end
                end
            end
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 2: –§–∞—Ä–º –ø—ã–ª—å—Ü—ã
            local fieldPos = GetFieldCenter()
            
            -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â—É—é —Ç–æ—á–∫—É
            local targetPos
            if Settings.WalkMode == "Circle" then
                local angle = step * 0.1
                targetPos = fieldPos + Vector3.new(
                    math.cos(angle) * Settings.FarmRadius,
                    0,
                    math.sin(angle) * Settings.FarmRadius
                )
                step = step + 1
                
            elseif Settings.WalkMode == "Square" then
                local side = math.floor(step / 20) % 4
                local pos = step % (Settings.FarmRadius * 2)
                
                if side == 0 then
                    targetPos = fieldPos + Vector3.new(pos - Settings.FarmRadius, 0, -Settings.FarmRadius)
                elseif side == 1 then
                    targetPos = fieldPos + Vector3.new(Settings.FarmRadius, 0, pos - Settings.FarmRadius)
                elseif side == 2 then
                    targetPos = fieldPos + Vector3.new(Settings.FarmRadius - pos, 0, Settings.FarmRadius)
                else
                    targetPos = fieldPos + Vector3.new(-Settings.FarmRadius, 0, Settings.FarmRadius - pos)
                end
                step = step + 10
                
            else -- Random
                targetPos = fieldPos + Vector3.new(
                    math.random(-Settings.FarmRadius, Settings.FarmRadius),
                    0,
                    math.random(-Settings.FarmRadius, Settings.FarmRadius)
                )
            end
            
            -- –î–≤–∏–≥–∞–µ–º—Å—è –∏ —Å–æ–±–∏—Ä–∞–µ–º –ø—ã–ª—å—Ü—É
            humanoid:MoveTo(targetPos)
            
            -- –í–´–ó–´–í–ê–ï–ú LMB –°–ë–û–†
            CollectPollen()
            
            -- –ü–†–ò–û–†–ò–¢–ï–¢ 3: –¢–æ–∫–µ–Ω—ã —Ä—è–¥–æ–º (–µ—Å–ª–∏ –Ω–µ –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ)
            if Settings.AutoTokens and not Settings.TokenPriority then
                local token, distance = FindNearestToken()
                if token and distance < 20 then
                    CollectToken(token)
                end
            end
        end
    end)
end

function GetFieldCenter()
    -- –°–Ω–∞—á–∞–ª–∞ –∏—â–µ–º —Ç–æ—á–Ω–æ–µ –ø–æ–ª–µ
    local field = workspace:FindFirstChild(Settings.CurrentField)
    if field and field:IsA("BasePart") then
        return field.Position
    end
    
    -- –ò—â–µ–º –≤ FlowerZones
    local flowerZones = workspace:FindFirstChild("FlowerZones")
    if flowerZones then
        for _, zone in pairs(flowerZones:GetChildren()) do
            if zone.Name:find(Settings.CurrentField) then
                if zone:IsA("BasePart") then
                    return zone.Position + Vector3.new(0, 5, 0)
                elseif zone:IsA("Model") and zone.PrimaryPart then
                    return zone.PrimaryPart.Position + Vector3.new(0, 5, 0)
                end
            end
        end
    end
    
    -- –î–µ—Ñ–æ–ª—Ç–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã
    local defaults = {
        ["Sunflower Field"] = Vector3.new(0, 10, 0),
        ["Pineapple Patch"] = Vector3.new(100, 10, 0),
        ["Bamboo Field"] = Vector3.new(-100, 10, 0),
        ["Mushroom Field"] = Vector3.new(0, 10, 100)
    }
    
    return defaults[Settings.CurrentField] or Vector3.new(0, 10, 0)
end

function TeleportToField(fieldName)
    local pos = GetFieldCenter()
    local character = Player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –ù–ê–î –ø–æ–ª–µ–º (–Ω–µ –ø–æ–¥)
        character.HumanoidRootPart.CFrame = CFrame.new(pos + Vector3.new(0, 5, 0))
        print("[–¢–ï–õ–ï–ü–û–†–¢] –ù–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function ShowStats()
    pcall(function()
        print("\n" .. string.rep("=", 40))
        print("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê:")
        
        -- –¢–æ–∫–µ–Ω—ã Red Boost
        local tokens = FindTokens()
        print("–¢–æ–∫–µ–Ω–æ–≤ Red Boost –Ω–∞ –∫–∞—Ä—Ç–µ: " .. #tokens)
        
        -- –ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω
        local nearest, dist = FindNearestToken()
        if nearest then
            print("–ë–ª–∏–∂–∞–π—à–∏–π —Ç–æ–∫–µ–Ω: " .. nearest.Name .. " (" .. math.floor(dist) .. " studs)")
        end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –¥–æ—Å—Ç—É–ø –∫ –∏–≥—Ä–æ–≤—ã–º –¥–∞–Ω–Ω—ã–º
        if Player:FindFirstChild("leaderstats") then
            local leaderstats = Player.leaderstats
            if leaderstats:FindFirstChild("Pollen") then
                print("–ü—ã–ª—å—Ü–∞: " .. leaderstats.Pollen.Value)
            end
        end
        
        -- –¢–∞–∫–∂–µ –ø—Ä–æ–≤–µ—Ä—è–µ–º PlayerStats
        if Player:FindFirstChild("PlayerStats") then
            local stats = Player.PlayerStats
            if stats:FindFirstChild("Pollen") then
                print("–ü—ã–ª—å—Ü–∞ (–∏–∑ PlayerStats): " .. stats.Pollen.Value)
            end
        end
        
        print(string.rep("=", 40))
    end)
end

-- –¢–µ—Å—Ç–∏—Ä—É–µ–º LMB —Å–±–æ—Ä –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
spawn(function()
    wait(3)
    print("\n[–¢–ï–°–¢] –ü—Ä–æ–±—É—é LMB —Å–±–æ—Ä –ø—ã–ª—å—Ü—ã...")
    local success = CollectPollen()
    print("[–¢–ï–°–¢] LMB —Å–±–æ—Ä: " .. (success and "–†–ê–ë–û–¢–ê–ï–¢" or "–ù–ï –†–ê–ë–û–¢–ê–ï–¢"))
    
    -- –ï—Å–ª–∏ –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç, –ø–æ–∫–∞–∂–µ–º –≤–æ–∑–º–æ–∂–Ω—ã–µ —Å–æ–±—ã—Ç–∏—è
    if not success then
        print("[–ü–û–ú–û–©–¨] –ü—Ä–æ–≤–µ—Ä—è—é –¥–æ—Å—Ç—É–ø–Ω—ã–µ RemoteEvents...")
        pcall(function()
            local events = game:GetService("ReplicatedStorage"):FindFirstChild("Events")
            if events then
                print("–î–æ—Å—Ç—É–ø–Ω—ã–µ —Å–æ–±—ã—Ç–∏—è –≤ Events:")
                for _, event in pairs(events:GetChildren()) do
                    if event:IsA("RemoteEvent") then
                        print("  - " .. event.Name)
                    end
                end
            end
        end)
    end
end)

-- –ê–≤—Ç–æ–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ SpeedHack
Player.CharacterAdded:Connect(function()
    wait(1)
    ApplySpeedHack()
end)

-- –ü—Ä–∏–º–µ–Ω—è–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
ApplySpeedHack()

