local currentY = 0

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–∞
local function AddElement(element, height)
    element.Position = UDim2.new(0, 0, 0, currentY)
    currentY = currentY + height
    return element
end

-- SpeedHack —Å–ª–∞–π–¥–µ—Ä—ã
local function CreateSpeedControl(label, min, max, default, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 0, 50)
    AddElement(frame, 0)
    frame.Parent = ScrollFrame
    
    local labelText = Instance.new("TextLabel")
    labelText.Text = label
    labelText.Size = UDim2.new(0.6, 0, 0, 20)
    labelText.Position = UDim2.new(0, 0, 0, 0)
    labelText.BackgroundTransparency = 1
    labelText.TextColor3 = Color3.fromRGB(200, 200, 255)
    labelText.Font = Enum.Font.Gotham
    labelText.TextSize = 12
    labelText.TextXAlignment = Enum.TextXAlignment.Left
    labelText.Parent = frame
    
    local valueText = Instance.new("TextLabel")
    valueText.Text = tostring(default)
    valueText.Size = UDim2.new(0.4, 0, 0, 20)
    valueText.Position = UDim2.new(0.6, 0, 0, 0)
    valueText.BackgroundTransparency = 1
    valueText.TextColor3 = Color3.fromRGB(255, 215, 0)
    valueText.Font = Enum.Font.GothamBold
    valueText.TextSize = 12
    valueText.Parent = frame
    
    local slider = Instance.new("Frame")
    slider.Size = UDim2.new(1, 0, 0, 6)
    slider.Position = UDim2.new(0, 0, 0, 25)
    slider.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    slider.Parent = frame
    
    local fill = Instance.new("Frame")
    fill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
    fill.BackgroundColor3 = Color3.fromRGB(255, 215, 0)
    fill.Parent = slider
    
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 16, 0, 16)
    button.Position = UDim2.new((default - min) / (max - min), -8, 0.5, -8)
    button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    button.Text = ""
    button.Parent = slider
    
    -- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    local function update(value)
        value = math.clamp(value, min, max)
        valueText.Text = tostring(math.floor(value))
        fill.Size = UDim2.new((value - min) / (max - min), 0, 1, 0)
        button.Position = UDim2.new((value - min) / (max - min), -8, 0.5, -8)
        if callback then callback(value) end
    end
    
    -- –ü–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ
    local dragging = false
    button.MouseButton1Down:Connect(function() dragging = true end)
    UIS.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end end)
    
    UIS.InputChanged:Connect(function(i)
        if dragging and i.UserInputType == Enum.UserInputType.MouseMovement then
            local pos = i.Position.X - slider.AbsolutePosition.X
            local percent = math.clamp(pos / slider.AbsoluteSize.X, 0, 1)
            update(min + (percent * (max - min)))
        end
    end)
    
    -- –ö–Ω–æ–ø–∫–∏ +/-
    local minus = Instance.new("TextButton")
    minus.Text = "-"
    minus.Size = UDim2.new(0, 30, 0, 20)
    minus.Position = UDim2.new(0, 0, 0, 35)
    minus.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
    minus.TextColor3 = Color3.fromRGB(255, 255, 255)
    minus.Font = Enum.Font.GothamBold
    minus.TextSize = 12
    minus.Parent = frame
    
    local plus = Instance.new("TextButton")
    plus.Text = "+"
    plus.Size = UDim2.new(0, 30, 0, 20)
    plus.Position = UDim2.new(1, -30, 0, 35)
    plus.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
    plus.TextColor3 = Color3.fromRGB(255, 255, 255)
    plus.Font = Enum.Font.GothamBold
    plus.TextSize = 12
    plus.Parent = frame
    
    minus.MouseButton1Click:Connect(function() update(tonumber(valueText.Text) - 5) end)
    plus.MouseButton1Click:Connect(function() update(tonumber(valueText.Text) + 5) end)
    
    return valueText
end

-- –°–æ–∑–¥–∞—ë–º —ç–ª–µ–º–µ–Ω—Ç—ã
local walkSpeedControl = CreateSpeedControl("‚ö° –°–∫–æ—Ä–æ—Å—Ç—å", 16, 200, 50, function(v)
    Settings.WalkSpeed = v
    ApplySpeedHack()
end)

local jumpPowerControl = CreateSpeedControl("ü¶ò –ü—Ä—ã–∂–æ–∫", 50, 200, 50, function(v)
    Settings.JumpPower = v
    ApplySpeedHack()
end)

-- –†–∞–∑–¥–µ–ª–∏—Ç–µ–ª—å
AddElement(Instance.new("Frame"), 10).BackgroundTransparency = 1

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª–∏
local function CreateToggle(text, yOffset, initialState, callback)
    local frame = AddElement(Instance.new("Frame"), 30)
    frame.Size = UDim2.new(1, 0, 0, 30)
    frame.Parent = ScrollFrame
    
    local label = Instance.new("TextLabel")
    label.Text = text
    label.Size = UDim2.new(0.7, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.TextColor3 = Color3.fromRGB(200, 200, 255)
    label.Font = Enum.Font.Gotham
    label.TextSize = 12
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = frame
    
    local button = Instance.new("TextButton")
    button.Text = initialState and "–í–ö–õ" or "–í–´–ö–õ"
    button.Size = UDim2.new(0, 50, 0, 20)
    button.Position = UDim2.new(1, -50, 0.5, -10)
    button.BackgroundColor3 = initialState and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 10
    button.Parent = frame
    
    button.MouseButton1Click:Connect(function()
        local newState = button.Text == "–í–´–ö–õ"
        button.Text = newState and "–í–ö–õ" or "–í–´–ö–õ"
        button.BackgroundColor3 = newState and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
        if callback then callback(newState) end
    end)
    
    return button
end

local farmToggle = CreateToggle("üåª –ê–≤—Ç–æ—Ñ–∞—Ä–º (LMB)", 0, false, function(state)
    Settings.AutoFarm = state
    if state then 
        StartAutoFarm()
        print("[–§–ê–†–ú] –ó–∞–ø—É—â–µ–Ω")
    else
        print("[–§–ê–†–ú] –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)

