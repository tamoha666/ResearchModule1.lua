    LastTokenScan = 0,
    TokenScanInterval = 1, -- –°–∫–∞–Ω–∏—Ä—É–µ–º —Ç–æ–∫–µ–Ω—ã —Ä–∞–∑ –≤ —Å–µ–∫—É–Ω–¥—É
    Tokens = {},
    LastFieldCheck = 0,
    FieldObjects = {},
    CurrentTarget = nil
}

