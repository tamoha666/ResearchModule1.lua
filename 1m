    WalkSpeed = 50,
    JumpPower = 50,
    AutoFarm = false,
    AutoTokens = false,
    CurrentField = "Sunflower Field",
    WalkMode = "Random",
    FarmRadius = 50,
    TokenPriority = true, -- –¢–æ–∫–µ–Ω—ã –≤ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–µ
    PerformanceMode = true -- –†–µ–∂–∏–º –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
}

