    Callback = function()
        AutoCollect = true
        AutoAttack = true
        AutoBuyBees = true
        StartAutoCollect()
        StartAutoAttack()
        StartAutoBuyBees()
        Rayfield:Notify({
            Title = "–í—Å—ë –∑–∞–ø—É—â–µ–Ω–æ!",
            Content = "–í—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã",
            Duration = 3,
            Image = "rbxassetid://4483345998"
        })
    end,
})

ControlTab:CreateButton({
    Name = "‚è∏Ô∏è –ü–∞—É–∑–∞ –≤—Å–µ–≥–æ",
    Callback = function()
        AutoCollect = false
        AutoAttack = false
        AutoBuyBees = false
        Rayfield:Notify({
            Title = "–í—Å—ë –Ω–∞ –ø–∞—É–∑–µ",
