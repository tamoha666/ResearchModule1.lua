-- Bee Swarm Simulator Auto Farmer v1.0 - Guaranteed Version
-- –í—Å—Ç–∞–≤—å –≤ –∏–Ω–∂–µ–∫—Ç–æ—Ä: loadstring(game:HttpGet('https://raw.githubusercontent.com/tamoha666/ResearchModule1.lua/refs/heads/main/1m'))()

local Player = game:GetService("Players").LocalPlayer
local Mouse = Player:GetMouse()

-- –°–æ–∑–¥–∞—ë–º —Å–≤–æ—ë –æ–∫–Ω–æ (–≤–º–µ—Å—Ç–æ Rayfield)
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "BeeFarmGUI"
ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 350, 0, 400)
MainFrame.Position = UDim2.new(0.5, -175, 0.5, -200)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Text = "üêù BEE SWARM AUTOFARMER üêù"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
Title.TextColor3 = Color3.fromRGB(255, 215, 0)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 18
Title.Parent = MainFrame

-- –¢–∞–±–ª–∏—á–∫–∏ —Å–æ —Å—Ç–∞—Ç—É—Å–∞–º–∏ —Ñ—É–Ω–∫—Ü–∏–π
local StatusLabels = {}
local Functions = {
    AutoCollect = {running = false, text = "‚úÖ –ê–≤—Ç–æ—Å–±–æ—Ä –ø—ã–ª—å—Ü—ã: –í–´–ö–õ"},
    AutoAttack = {running = false, text = "‚öîÔ∏è –ê–≤—Ç–æ–∞—Ç–∞–∫–∞ –º–æ–±–æ–≤: –í–´–ö–õ", target = "Ladybug"},
    AutoBuyBees = {running = false, text = "üõí –ê–≤—Ç–æ–ø–æ–∫—É–ø–∫–∞ –ø—á—ë–ª: –í–´–ö–õ"}
}

-- –°–æ–∑–¥–∞—ë–º –∫–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
local buttonPositions = {
    {name = "‚ñ∂Ô∏è –°—Ç–∞—Ä—Ç —Å–±–æ—Ä–∞", func = "AutoCollect", yPos = 50},
    {name = "‚öîÔ∏è –°—Ç–∞—Ä—Ç –∞—Ç–∞–∫–∏", func = "AutoAttack", yPos = 90},
    {name = "üõí –ö—É–ø–∏—Ç—å –ø—á—ë–ª", func = "AutoBuyBees", yPos = 130},
    {name = "‚è∏Ô∏è –í–°–Å –°–¢–û–ü", func = "STOPALL", yPos = 170},
    {name = "üéØ –¶–µ–ª—å: Ladybug", func = "CHANGE_TARGET", yPos = 210},
    {name = "üìä –û–±–Ω–æ–≤–∏—Ç—å —Å—Ç–∞—Ç—É—Å", func = "UPDATE_STATS", yPos = 250},
    {name = "‚ùå –ó–∞–∫—Ä—ã—Ç—å GUI", func = "CLOSE_GUI", yPos = 290}
}

for i, btnInfo in ipairs(buttonPositions) do
    local button = Instance.new("TextButton")
    button.Text = btnInfo.name
    button.Size = UDim2.new(0.9, 0, 0, 35)
    button.Position = UDim2.new(0.05, 0, 0, btnInfo.yPos)
    button.BackgroundColor3 = Color3.fromRGB(60, 60, 75)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.Gotham
    button.TextSize = 14
    button.Parent = MainFrame
    
    -- –°—Ç–∞—Ç—É—Å-–ª–µ–π–±–ª —Ä—è–¥–æ–º —Å –∫–Ω–æ–ø–∫–æ–π
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Text = ""
    statusLabel.Size = UDim2.new(0.9, 0, 0, 20)
    statusLabel.Position = UDim2.new(0.05, 0, 0, btnInfo.yPos + 35)
    statusLabel.BackgroundTransparency = 1
    statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextSize = 12
    statusLabel.Parent = MainFrame
    
    StatusLabels[btnInfo.func] = statusLabel
    
    -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏–π
    button.MouseButton1Click:Connect(function()
        if btnInfo.func == "STOPALL" then
            for funcName in pairs(Functions) do
                Functions[funcName].running = false
                StatusLabels[funcName].Text = Functions[funcName].text
            end
            print("[SYSTEM] –í—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã")
            
        elseif btnInfo.func == "CHANGE_TARGET" then
            local mobs = {"Ladybug", "Rhino Beetle", "Mantis", "Spider"}
            local current = Functions.AutoAttack.target
            local nextIndex = 1
            for i, mob in ipairs(mobs) do
                if mob == current then
                    nextIndex = (i % #mobs) + 1
                    break
                end
            end
            Functions.AutoAttack.target = mobs[nextIndex]
            button.Text = "üéØ –¶–µ–ª—å: " .. mobs[nextIndex]
            print("[SYSTEM] –¶–µ–ª—å –∏–∑–º–µ–Ω–µ–Ω–∞ –Ω–∞: " .. mobs[nextIndex])
            
        elseif btnInfo.func == "UPDATE_STATS" then
            UpdateDisplayStats()
            
        elseif btnInfo.func == "CLOSE_GUI" then
            ScreenGui:Destroy()
            print("[SYSTEM] –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å –∑–∞–∫—Ä—ã—Ç")
            
        elseif Functions[btnInfo.func] then
            Functions[btnInfo.func].running = not Functions[btnInfo.func].running
            StatusLabels[btnInfo.func].Text = Functions[btnInfo.func].running and "üü¢ –†–ê–ë–û–¢–ê–ï–¢" or "üî¥ –í–´–ö–õ"
            
            if Functions[btnInfo.func].running then
                print("[SYSTEM] –ó–∞–ø—É—â–µ–Ω–æ: " .. btnInfo.func)
                if btnInfo.func == "AutoCollect" then StartAutoCollect() end
                if btnInfo.func == "AutoAttack" then StartAutoAttack() end
                if btnInfo.func == "AutoBuyBees" then StartAutoBuyBees() end
            else
                print("[SYSTEM] –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ: " .. btnInfo.func)
            end
        end
    end)
end

-- –§—É–Ω–∫—Ü–∏–∏ —Ñ–∞—Ä–º–∞ (–æ—Å—Ç–∞–≤–ª—è–µ–º —Ä–∞–±–æ—á—É—é –ª–æ–≥–∏–∫—É, —É–±–∏—Ä–∞–µ–º —Å–ª–æ–∂–Ω–æ—Å—Ç–∏)
function StartAutoCollect()
    spawn(function()
        while Functions.AutoCollect.running do
            pcall(function()
                -- –ü—Ä–æ—Å—Ç–æ –∫–ª–∏–∫–∞–µ–º –ø–æ —Ü–≤–µ—Ç–∫–∞–º –≤–æ–∫—Ä—É–≥
                local character = Player.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    -- –ò—â–µ–º –±–ª–∏–∂–∞–π—à–∏–π —Ü–≤–µ—Ç–æ–∫
                    local nearestFlower = FindNearestObject("Flower")
                    if nearestFlower and nearestFlower:FindFirstChild("Pollen") then
                        -- –ö–ª–∏–∫–∞–µ–º –ø–æ –Ω–µ–º—É
                        fireproximityprompt(nearestFlower.Pollen.ProximityPrompt)
                    end
                end
            end)
            wait(0.3) -- –ò–Ω—Ç–µ—Ä–≤–∞–ª –∫–ª–∏–∫–æ–≤
        end
    end)
end

function StartAutoAttack()
    spawn(function()
        while Functions.AutoAttack.running do
            pcall(function()
                local targetName = Functions.AutoAttack.target
                local mob = FindMob(targetName)
                
                if mob and mob:FindFirstChild("HumanoidRootPart") then
                    -- –ê—Ç–∞–∫—É–µ–º
                    game:GetService("ReplicatedStorage").Events.PlayerClick:FireServer(
                        mob.HumanoidRootPart.Position,
                        "Attack"
                    )
                end
            end)
            wait(0.5)
        end
    end)
end

function StartAutoBuyBees()
    spawn(function()
        while Functions.AutoBuyBees.running do
            pcall(function()
                -- –ü—Ä–æ—Å—Ç–æ –æ—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å –Ω–∞ –ø–æ–∫—É–ø–∫—É –ø—á–µ–ª—ã (–µ—Å–ª–∏ –µ—Å—Ç—å –º–µ—Å—Ç–æ)
                game:GetService("ReplicatedStorage").Events.BeePurchaseRequest:FireServer()
            end)
            wait(2) -- –ü–æ–∫—É–ø–∞–µ–º —Ä–∞–∑ –≤ 2 —Å–µ–∫—É–Ω–¥—ã
        end
    end)
end

-- –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
function FindNearestObject(objectName)
    local character = Player.Character
    if not character then return nil end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return nil end
    
    local nearest = nil
    local nearestDist = math.huge
    
    for _, obj in pairs(workspace:GetChildren()) do
        if obj.Name:find(objectName) and obj:FindFirstChild("Pollen") then
            local dist = (root.Position - obj.Position).Magnitude
            if dist < nearestDist and dist < 50 then
                nearest = obj
                nearestDist = dist
            end
        end
    end
    return nearest
end

function FindMob(mobName)
    for _, mob in pairs(workspace.Mobs:GetChildren()) do
        if mob.Name:find(mobName) and mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
            return mob
        end
    end
    return nil
end

function UpdateDisplayStats()
    pcall(function()
        -- –ü—Ä–æ—Å—Ç–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
        local hive = workspace.Hives:FindFirstChild(Player.Name)
        if hive and hive:FindFirstChild("Bees") then
            StatusLabels.UPDATE_STATS.Text = "–ü—á—ë–ª –≤ —É–ª—å–µ: " .. #hive.Bees:GetChildren()
        else
            StatusLabels.UPDATE_STATS.Text = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω–∞"
        end
    end)
end

-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –∑–∞–ø—É—Å–∫–µ
StatusLabels.AutoCollect.Text = "üî¥ –í–´–ö–õ"
StatusLabels.AutoAttack.Text = "üî¥ –í–´–ö–õ"
StatusLabels.AutoBuyBees.Text = "üî¥ –í–´–ö–õ"
StatusLabels.UPDATE_STATS.Text = "–ù–∞–∂–º–∏ –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è"

