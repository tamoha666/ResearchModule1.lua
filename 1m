    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field"}
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            Settings.CurrentField = fields[(i % #fields) + 1]
            break
        end
    end
    fieldButton.Text = "üåç –ü–æ–ª–µ: " .. Settings.CurrentField
    print("[–ü–û–õ–ï] –í—ã–±—Ä–∞–Ω–æ: " .. Settings.CurrentField)
    Cache.FieldPosition = nil -- –°–±—Ä–æ—Å–∏—Ç—å –∫—ç—à –ø–æ–∑–∏—Ü–∏–∏ –ø–æ–ª—è
end)
currentY = currentY + 40

