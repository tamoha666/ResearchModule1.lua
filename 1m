-- –ö–Ω–æ–ø–∫–∞ –≤—ã–±–æ—Ä–∞ –ø–æ–ª—è (—Å –æ–±–Ω–æ–≤–ª—è–µ–º—ã–º —Ç–µ–∫—Å—Ç–æ–º)
local fieldButton = CreateButton("üåç –ü–æ–ª–µ: " .. Settings.CurrentField, currentY, function(btn)
    local fields = {"Sunflower Field", "Pineapple Patch", "Bamboo Field", "Mushroom Field", "Clover Field"}
    local currentIndex = 1
    for i, field in ipairs(fields) do
        if field == Settings.CurrentField then
            currentIndex = i
            break
        end
    end
    local nextIndex = (currentIndex % #fields) + 1
    Settings.CurrentField = fields[nextIndex]
    btn.Text = "üåç –ü–æ–ª–µ: " .. Settings.CurrentField
    print("[–ü–û–õ–ï] –í—ã–±—Ä–∞–Ω–æ: " .. Settings.CurrentField)
end, Color3.fromRGB(80, 160, 80), true)
currentY = currentY + 50

-- –ö–Ω–æ–ø–∫–∞ –≤—ã–±–æ—Ä–∞ —Ä–µ–∂–∏–º–∞ (—Å –æ–±–Ω–æ–≤–ª—è–µ–º—ã–º —Ç–µ–∫—Å—Ç–æ–º)
local modeButton = CreateButton("üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode, currentY, function(btn)
    local modes = {"Random", "Circle", "Square", "ZigZag"}
    local currentIndex = 1
    for i, mode in ipairs(modes) do
        if mode == Settings.WalkMode then
            currentIndex = i
            break
        end
    end
    local nextIndex = (currentIndex % #modes) + 1
    Settings.WalkMode = modes[nextIndex]
    btn.Text = "üö∂ –†–µ–∂–∏–º: " .. Settings.WalkMode
    print("[–†–ï–ñ–ò–ú] –£—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: " .. Settings.WalkMode)
end, nil, true)
currentY = currentY + 50

