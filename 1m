    print("[DEBUG] –†–µ–∂–∏–º –æ—Ç–ª–∞–¥–∫–∏: " .. (Settings.DebugMode and "–í–ö–õ" or "–í–´–ö–õ"))
end)
currentY = currentY + 50

-- –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞
