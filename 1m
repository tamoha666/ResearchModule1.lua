    Name = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å —Å–±–æ—Ä–∞",
    Range = {0.05, 1},
    Increment = 0.05,
    Suffix = "—Å–µ–∫",
    CurrentValue = 0.1,
    Flag = "CollectSpeedSlider",
    Callback = function(Value)
        CollectSpeed = Value
        Rayfield:Notify({
            Title = "–°–∫–æ—Ä–æ—Å—Ç—å –∏–∑–º–µ–Ω–µ–Ω–∞",
            Content = "–ù–æ–≤–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å: " .. Value .. " —Å–µ–∫",
            Duration = 1.5,
            Image = "rbxassetid://4483345998"
        })
    end,
})

-- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
local InfoTab = Window:CreateTab("üìà –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è", "rbxassetid://4483345998")

local StatsLabel = InfoTab:CreateLabel("üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ —Ñ–∞—Ä–º–∞:")
local PollenCountLabel = InfoTab:CreateLabel("–ü—ã–ª—å—Ü–∞: 0")
local BeesCountLabel = InfoTab:CreateLabel("–ü—á—ë–ª—ã: 0")
local MobsKilledLabel = InfoTab:CreateLabel("–ú–æ–±–æ–≤ —É–±–∏—Ç–æ: 0")
local RuntimeLabel = InfoTab:CreateLabel("–í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã: 00:00:00")

-- –ö–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
local ControlTab = Window:CreateTab("üéÆ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ", "rbxassetid://4483345998")

