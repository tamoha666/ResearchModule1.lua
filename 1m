print("1. Пробуем загрузить Rayfield...")
local success, Rayfield = pcall(function()
    return loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
end)

if success and Rayfield then
    print("✅ Rayfield загружен!")
    warn(Rayfield) -- Посмотрим, что вернулось
else
    print("❌ Ошибка загрузки Rayfield!")
    warn(Rayfield) -- Здесь будет текст ошибки
end
