-- –§–£–ù–ö–¶–ò–ò –ê–í–¢–û–§–ê–†–ú–ê

function StartAutoCollect()
    spawn(function()
        while Settings.AutoCollect do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local humanoid = character:FindFirstChild("Humanoid")
                local root = character:FindFirstChild("HumanoidRootPart")
                if not humanoid or not root then return end
                
                -- –ò—â–µ–º –±–ª–∏–∂–∞–π—à–∏–π —Ü–≤–µ—Ç–æ–∫
                local nearestFlower = nil
                local nearestDistance = Settings.CollectRange
                
                for _, obj in pairs(workspace:GetChildren()) do
                    if obj.Name == "Flower" or obj.Name:find("Field") then
                        if obj:FindFirstChild("Pollen") then
                            local distance = (root.Position - obj.Position).Magnitude
                            if distance < nearestDistance then
                                nearestFlower = obj
                                nearestDistance = distance
                            end
                        end
                    end
                end
                
                -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ —Ü–≤–µ—Ç–æ–∫ - –∏–¥—ë–º –∫ –Ω–µ–º—É –∏ —Å–æ–±–∏—Ä–∞–µ–º
                if nearestFlower then
                    -- –î–≤–∏–≥–∞–µ–º—Å—è –∫ —Ü–≤–µ—Ç–∫—É
                    humanoid:MoveTo(nearestFlower.Position)
                    
                    -- –ñ–¥—ë–º –ø–æ–∫–∞ –ø–æ–¥–æ–π–¥—ë–º –±–ª–∏–∑–∫–æ
                    wait(0.2)
                    
                    -- –ö–ª–∏–∫–∞–µ–º –ø–æ —Ü–≤–µ—Ç–∫—É (–∏–º–∏—Ç–∏—Ä—É–µ–º LMB)
                    if nearestFlower:FindFirstChild("Pollen") then
                        fireproximityprompt(nearestFlower.Pollen.ProximityPrompt)
                        
                        -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–π –∫–ª–∏–∫ —á–µ—Ä–µ–∑ —Ä–µ–ø–ª–∏—Ü–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ
                        game:GetService("ReplicatedStorage").Events.PlayerClick:FireServer(
                            nearestFlower.Position,
                            "Collect"
                        )
                    end
                else
                    -- –ï—Å–ª–∏ —Ü–≤–µ—Ç–∫–æ–≤ –Ω–µ—Ç —Ä—è–¥–æ–º, –ø–µ—Ä–µ–º–µ—â–∞–µ–º—Å—è —Å–ª—É—á–∞–π–Ω–æ
                    local randomPos = root.Position + Vector3.new(
                        math.random(-50, 50),
                        0,
                        math.random(-50, 50)
                    )
                    humanoid:MoveTo(randomPos)
                end
            end)
            wait(Settings.FarmSpeed)
        end
    end)
end

function StartBoostTokenCollect()
    spawn(function()
        while Settings.AutoBoostTokens do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local root = character:FindFirstChild("HumanoidRootPart")
                if not root then return end
                
                -- –ò—â–µ–º —Ç–æ–∫–µ–Ω—ã –±—É—Å—Ç–æ–≤
                for _, obj in pairs(workspace:GetChildren()) do
                    if obj.Name:find("Token") or obj.Name:find("Boost") then
                        if obj:IsA("BasePart") then
                            local distance = (root.Position - obj.Position).Magnitude
                            if distance < Settings.CollectRange then
                                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ —Ç–æ–∫–µ–Ω—É
                                root.CFrame = CFrame.new(obj.Position + Vector3.new(0, 3, 0))
                                wait(0.1)
                                
                                -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º —Ç–æ–∫–µ–Ω
                                firetouchinterest(root, obj, 0)
                                firetouchinterest(root, obj, 1)
                            end
                        end
                    end
                end
            end)
            wait(0.3)
        end
    end)
end

function StartAutoAttack()
    spawn(function()
        while Settings.AutoAttack do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local humanoid = character:FindFirstChild("Humanoid")
                local root = character:FindFirstChild("HumanoidRootPart")
                if not humanoid or not root then return end
                
                -- –ò—â–µ–º –º–æ–±–∞
                local targetMob = nil
                for _, mob in pairs(workspace.Mobs:GetChildren()) do
                    if mob.Name:find(Settings.TargetMob) then
                        if mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
                            targetMob = mob
                            break
                        end
                    end
                end
                
                if targetMob and targetMob:FindFirstChild("HumanoidRootPart") then
                    -- –î–≤–∏–≥–∞–µ–º—Å—è –∫ –º–æ–±—É
                    humanoid:MoveTo(targetMob.HumanoidRootPart.Position)
                    wait(0.2)
                    
                    -- –ê—Ç–∞–∫—É–µ–º
                    game:GetService("ReplicatedStorage").Events.PlayerClick:FireServer(
                        targetMob.HumanoidRootPart.Position,
                        "Attack"
                    )
                    
                    -- –ù–µ–ø—Ä–µ—Ä—ã–≤–Ω–∞—è –∞—Ç–∞–∫–∞ –ø–æ–∫–∞ –∂–∏–≤
                    for i = 1, 5 do
                        if not Settings.AutoAttack then break end
                        if targetMob.Humanoid.Health > 0 then
                            game:GetService("ReplicatedStorage").Events.PlayerClick:FireServer(
                                targetMob.HumanoidRootPart.Position,
                                "Attack"
                            )
                            wait(0.2)
                        else
                            break
                        end
                    end
                end
            end)
            wait(0.5)
        end
    end)
end

function ApplySpeedHack()
    pcall(function()
        local character = Player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = Settings.WalkSpeed
                humanoid.JumpPower = Settings.JumpPower
            end
        end
    end)
end

-- –£—Ç–∏–ª–∏—Ç—ã
function TeleportToSpawn()
    pcall(function()
        local spawn = workspace:FindFirstChild("SpawnPos") or workspace:FindFirstChild("Spawn")
        if spawn then
            Player.Character:SetPrimaryPartCFrame(spawn.CFrame)
            print("[–¢–ï–õ–ï–ü–û–†–¢] –ü–µ—Ä–µ–º–µ—â—ë–Ω –Ω–∞ —Å–ø–∞–≤–Ω")
        end
    end)
end

function TeleportToHive()
    pcall(function()
        local hive = workspace.Hives:FindFirstChild(Player.Name)
        if hive then
            Player.Character:SetPrimaryPartCFrame(hive.CFrame + Vector3.new(0, 5, 0))
            print("[–¢–ï–õ–ï–ü–û–†–¢] –ü–µ—Ä–µ–º–µ—â—ë–Ω –∫ —É–ª—å—é")
        end
    end)
end

function ShowStatistics()
    pcall(function()
        local hive = workspace.Hives:FindFirstChild(Player.Name)
        local stats = ""
        
        if hive and hive:FindFirstChild("Bees") then
            stats = stats .. "üêù –ü—á—ë–ª –≤ —É–ª—å–µ: " .. #hive.Bees:GetChildren() .. "\n"
        end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ PlayerStats
        if Player:FindFirstChild("PlayerStats") then
            local playerStats = Player.PlayerStats
            if playerStats:FindFirstChild("Pollen") then
                stats = stats .. "üåº –ü—ã–ª—å—Ü–∞: " .. playerStats.Pollen.Value .. "\n"
            end
            if playerStats:FindFirstChild("Honey") then
                stats = stats .. "üçØ –ú—ë–¥: " .. playerStats.Honey.Value .. "\n"
            end
        end
        
        print("\n" .. string.rep("=", 40))
        print("üìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê –ò–ì–†–û–ö–ê")
        print(stats)
        print(string.rep("=", 40))
    end)
end

function OptimizeGame()
    pcall(function()
        -- –£–º–µ–Ω—å—à–∞–µ–º –∫–∞—á–µ—Å—Ç–≤–æ –≥—Ä–∞—Ñ–∏–∫–∏ –¥–ª—è –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        settings().Rendering.QualityLevel = 1
        
        -- –û—Ç–∫–ª—é—á–∞–µ–º –Ω–µ–∫–æ—Ç–æ—Ä—ã–µ —ç—Ñ—Ñ–µ–∫—Ç—ã
        for _, effect in pairs(workspace:GetDescendants()) do
            if effect:IsA("ParticleEmitter") or effect:IsA("Trail") then
                effect.Enabled = false
            end
        end
        
        print("[–û–ü–¢–ò–ú–ò–ó–ê–¶–ò–Ø] –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –≥—Ä–∞—Ñ–∏–∫–∏ —É–º–µ–Ω—å—à–µ–Ω—ã –¥–ª—è FPS")
    end)
end

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏–º–µ–Ω—è–µ–º SpeedHack –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
Player.CharacterAdded:Connect(function()
    wait(1)
    ApplySpeedHack()
end)

