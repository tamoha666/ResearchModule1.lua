local targetButton = CreateButton("üéØ –¶–µ–ª—å: " .. Settings.TargetMob, currentY, function()
    local mobs = {"Ladybug", "Rhino Beetle", "Mantis", "Spider", "Scorpion"}
    local currentIndex = 1
    for i, mob in ipairs(mobs) do
        if mob == Settings.TargetMob then
            currentIndex = i
            break
        end
    end
    local nextIndex = (currentIndex % #mobs) + 1
    Settings.TargetMob = mobs[nextIndex]
    targetButton.Text = "üéØ –¶–µ–ª—å: " .. Settings.TargetMob
    print("[–ù–ê–°–¢–†–û–ô–ö–ò] –¶–µ–ª—å –∏–∑–º–µ–Ω–µ–Ω–∞ –Ω–∞: " .. Settings.TargetMob)
