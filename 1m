    WalkSpeed = 50,
    JumpPower = 50,
    AutoFarm = false,
    AutoTokens = false,
    ClickDelay = 0.05, -- –ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É –∫–ª–∏–∫–∞–º–∏
    TokenRange = 100,
    CurrentField = "Sunflower Field",
    FarmRadius = 100, -- –†–∞–¥–∏—É—Å —Ñ–∞—Ä–º–∞ –Ω–∞ –ø–æ–ª–µ
    WalkMode = "Random", -- Random, Circle, Square
    DebugMode = false
}

-- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –ø–æ–ª–µ–π (—Ü–µ–Ω—Ç—Ä—ã)
