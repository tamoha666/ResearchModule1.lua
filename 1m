-- Диагностика объектов вокруг
print("=== ДИАГНОСТИКА ОБЪЕКТОВ ===")

local function ScanObjects()
    local allObjects = {}
    
    -- Сканируем всё в Workspace
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") or obj:IsA("MeshPart") then
            local name = obj.Name:lower()
            
            -- Ищем токены и бусты
            if name:find("token") or name:find("boost") or name:find("orb") 
               or name:find("ball") or name:find("pickup") then
                table.insert(allObjects, {
                    Name = obj.Name,
                    Class = obj.ClassName,
                    Color = obj.Color,
                    Position = obj.Position
                })
            end
        end
    end
    
    return allObjects
end

-- Запускаем сканирование
local objects = ScanObjects()
print("Найдено объектов:", #objects)

for i, obj in ipairs(objects) do
    print(string.format("%d. %s (%s) - Цвет: %s", 
        i, obj.Name, obj.Class, 
        tostring(obj.Color)))
end

-- Автоматическое отслеживание новых объектов
workspace.DescendantAdded:Connect(function(obj)
    if obj:IsA("BasePart") then
        local name = obj.Name:lower()
        if name:find("token") or name:find("boost") then
            print("[НОВЫЙ ОБЪЕКТ] " .. obj.Name .. " - " .. obj.ClassName)
        end
    end
end)

print("\nПерейди на поле, и я покажу какие объекты появляются!")
