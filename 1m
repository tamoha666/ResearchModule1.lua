    ["Sunflower Field"] = Vector3.new(0, 0, 0), -- –ó–∞–º–µ–Ω–∏ –Ω–∞ —Ä–µ–∞–ª—å–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã!
    ["Pineapple Patch"] = Vector3.new(100, 0, 100),
    ["Bamboo Field"] = Vector3.new(-100, 0, 100),
    ["Mushroom Field"] = Vector3.new(0, 0, -100)
}

