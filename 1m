-- –§—É–Ω–∫—Ü–∏–∏ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞
function StartAutoCollect()
    spawn(function()
        while AutoCollect do
            pcall(function()
                -- –ò—â–µ–º —Ü–µ–ª–µ–≤–æ–µ –ø–æ–ª–µ
                local field = FindField(TargetField)
                if field then
                    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ –ø–æ–ª—é
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = field.CFrame + Vector3.new(0, 5, 0)
                    
                    -- –°–æ–±–∏—Ä–∞–µ–º –ø—ã–ª—å—Ü—É —Å —Ü–≤–µ—Ç–æ–≤
                    local flowers = GetFlowersInField(field)
                    for _, flower in pairs(flowers) do
                        if not AutoCollect then break end
                        if flower:FindFirstChild("Pollen") then
                            -- –ò–º–∏—Ç–∞—Ü–∏—è —Å–±–æ—Ä–∞
                            fireproximityprompt(flower.Pollen.ProximityPrompt)
                            wait(CollectSpeed)
                        end
                    end
                end
            end)
            wait(0.5)
        end
    end)
end

function StartAutoAttack()
    spawn(function()
        while AutoAttack do
            pcall(function()
                -- –ò—â–µ–º –º–æ–±–∞
                local mob = FindMob(TargetMob)
                if mob and mob:FindFirstChild("HumanoidRootPart") then
                    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ –º–æ–±—É
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = mob.HumanoidRootPart.CFrame + Vector3.new(0, 0, -5)
                    
                    -- –ê—Ç–∞–∫—É–µ–º
                    game:GetService("ReplicatedStorage").Events.PlayerClick:FireServer(
                        mob.HumanoidRootPart.Position,
                        "Attack"
                    )
                    
                    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
                    UpdateKillCount()
                end
            end)
            wait(0.3)
        end
    end)
end

function StartAutoBuyBees()
    spawn(function()
        while AutoBuyBees do
            pcall(function()
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –º–æ–∂–µ–º –ª–∏ –∫—É–ø–∏—Ç—å –ø—á–µ–ª—É
                local hive = game:GetService("Workspace").Hives:FindFirstChild(game.Players.LocalPlayer.Name)
                if hive then
                    local cells = hive:FindFirstChild("Cells")
                    if cells then
                        for _, cell in pairs(cells:GetChildren()) do
                            if cell:FindFirstChild("Empty") then
                                -- –ü–æ–∫—É–ø–∞–µ–º –ø—á–µ–ª—É
                                game:GetService("ReplicatedStorage").Events.BeePurchaseRequest:FireServer(cell)
                                wait(1)
                            end
                        end
                    end
                end
            end)
            wait(5) -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–∞–∂–¥—ã–µ 5 —Å–µ–∫—É–Ω–¥
        end
    end)
end

-- –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
function FindField(fieldName)
    local fields = game:GetService("Workspace").FlowerZones
    for _, field in pairs(fields:GetChildren()) do
        if field.Name == fieldName then
            return field
        end
    end
    return nil
end

function GetFlowersInField(field)
    local flowers = {}
    for _, obj in pairs(field:GetDescendants()) do
        if obj.Name == "Flower" and obj:FindFirstChild("Pollen") then
            table.insert(flowers, obj)
        end
    end
    return flowers
end

function FindMob(mobName)
    local mobs = game:GetService("Workspace").Mobs
    for _, mob in pairs(mobs:GetChildren()) do
        if mob.Name:find(mobName) and mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
            return mob
        end
    end
    return nil
end

-- –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞
local PollenCount = 0
local BeesCount = 0
local MobsKilled = 0
local StartTime = tick()

function UpdateStats()
    pcall(function()
        -- –ü–æ–ª—É—á–∞–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –ø—ã–ª—å—Ü—ã
        local playerStats = game:GetService("Players").LocalPlayer:FindFirstChild("PlayerStats")
        if playerStats then
            local pollen = playerStats:FindFirstChild("Pollen")
            if pollen then
                PollenCount = pollen.Value
            end
        end
        
        -- –°—á–∏—Ç–∞–µ–º –ø—á—ë–ª
        local hive = game:GetService("Workspace").Hives:FindFirstChild(game.Players.LocalPlayer.Name)
        if hive and hive:FindFirstChild("Bees") then
            BeesCount = #hive.Bees:GetChildren()
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –ª–µ–π–±–ª—ã
        PollenCountLabel:Set("–ü—ã–ª—å—Ü–∞: " .. tostring(PollenCount))
        BeesCountLabel:Set("–ü—á—ë–ª—ã: " .. tostring(BeesCount))
        MobsKilledLabel:Set("–ú–æ–±–æ–≤ —É–±–∏—Ç–æ: " .. tostring(MobsKilled))
        
        -- –í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã
        local elapsed = tick() - StartTime
        local hours = math.floor(elapsed / 3600)
        local minutes = math.floor((elapsed % 3600) / 60)
        local seconds = math.floor(elapsed % 60)
        RuntimeLabel:Set(string.format("–í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã: %02d:%02d:%02d", hours, minutes, seconds))
    end)
end

function UpdateKillCount()
    MobsKilled = MobsKilled + 1
    MobsKilledLabel:Set("–ú–æ–±–æ–≤ —É–±–∏—Ç–æ: " .. tostring(MobsKilled))
end

-- –ê–≤—Ç–æ-–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
spawn(function()
    while true do
        if AutoCollect or AutoAttack then
            UpdateStats()
        end
        wait(5)
    end
end)

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
Rayfield:Notify({
    Title = "Bee Swarm AutoFarmer",
    Content = "–£—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω! –í—ã–±–µ—Ä–∏ —Ñ—É–Ω–∫—Ü–∏–∏ –≤ –º–µ–Ω—é.",
    Duration = 5,
    Image = "rbxassetid://4483345998"
})

UpdateStats()

