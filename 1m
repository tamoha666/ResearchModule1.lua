end)

-- –†–ê–ë–û–ß–ò–ï –§–£–ù–ö–¶–ò–ò –ê–í–¢–û–§–ê–†–ú–ê

function StartAutoFarm()
    spawn(function()
        local step = 0
        local angle = 0
        
        while Settings.AutoFarm do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local humanoid = character:FindFirstChild("Humanoid")
                local root = character:FindFirstChild("HumanoidRootPart")
                if not humanoid or not root then return end
                
                -- –ü–æ–ª—É—á–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –ø–æ–ª—è
                local fieldPos = GetFieldPosition()
                
                -- –í—ã–±–∏—Ä–∞–µ–º —Å–ª–µ–¥—É—é—â—É—é —Ç–æ—á–∫—É –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä–µ–∂–∏–º–∞
                local targetPos
                if Settings.WalkMode == "Circle" then
                    targetPos = fieldPos + Vector3.new(
                        math.cos(angle) * Settings.FarmRadius,
                        0,
                        math.sin(angle) * Settings.FarmRadius
                    )
                    angle = angle + 0.2
                    
                elseif Settings.WalkMode == "Square" then
                    local side = math.floor(step / 20) % 4
                    local pos = step % (Settings.FarmRadius * 2)
                    
                    if side == 0 then
                        targetPos = fieldPos + Vector3.new(pos - Settings.FarmRadius, 0, -Settings.FarmRadius)
                    elseif side == 1 then
                        targetPos = fieldPos + Vector3.new(Settings.FarmRadius, 0, pos - Settings.FarmRadius)
                    elseif side == 2 then
                        targetPos = fieldPos + Vector3.new(Settings.FarmRadius - pos, 0, Settings.FarmRadius)
                    else
                        targetPos = fieldPos + Vector3.new(-Settings.FarmRadius, 0, Settings.FarmRadius - pos)
                    end
                    step = step + 10
                    
                elseif Settings.WalkMode == "ZigZag" then
                    local x = (step % (Settings.FarmRadius * 2)) - Settings.FarmRadius
                    local z = Settings.FarmRadius * math.sin(step / 20)
                    targetPos = fieldPos + Vector3.new(x, 0, z)
                    step = step + 15
                    
                else -- Random
                    targetPos = fieldPos + Vector3.new(
                        math.random(-Settings.FarmRadius, Settings.FarmRadius),
                        0,
                        math.random(-Settings.FarmRadius, Settings.FarmRadius)
                    )
                end
                
                -- –î–≤–∏–≥–∞–µ–º—Å—è –∫ —Ç–æ—á–∫–µ
                humanoid:MoveTo(targetPos)
                
                -- –î–µ–ª–∞–µ–º –∫–ª–∏–∫ –¥–ª—è —Å–±–æ—Ä–∞ –ø—ã–ª—å—Ü—ã (LMB)
                CollectPollen()
                
                -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞
                wait(0.3)
            end)
        end
    end)
end

function StartTokenCollect()
    spawn(function()
        while Settings.AutoTokens do
            pcall(function()
                local character = Player.Character
                if not character then return end
                
                local root = character:FindFirstChild("HumanoidRootPart")
                if not root then return end
                
                -- –ò—â–µ–º —Ç–æ–∫–µ–Ω—ã —Å –∏–º–µ–Ω–µ–º "Orb" (–∏–ª–∏ —Å–æ–¥–µ—Ä–∂–∞—â–∏–º "Orb")
                for _, obj in pairs(workspace:GetDescendants()) do
                    if not Settings.AutoTokens then break end
                    
                    if obj:IsA("BasePart") then
                        local name = obj.Name
                        if name:find("Orb") or name == Settings.TokenName then
                            local distance = (root.Position - obj.Position).Magnitude
                            
                            if distance < Settings.TokenRange then
                                -- –°–æ–±–∏—Ä–∞–µ–º —Ç–æ–∫–µ–Ω
                                CollectToken(obj)
                                
                                -- –î–µ–ª–∞–µ–º –ø–∞—É–∑—É –º–µ–∂–¥—É —Å–±–æ—Ä–∞–º–∏
                                wait(0.1)
                            end
                        end
                    end
                end
            end)
            wait(0.5) -- –°–∫–∞–Ω–∏—Ä—É–µ–º –∫–∞–∂–¥—ã–µ 0.5 —Å–µ–∫—É–Ω–¥
        end
    end)
end

function CollectPollen()
    -- –ú–µ—Ç–æ–¥ 1: –ß–µ—Ä–µ–∑ RemoteEvent (—Å–∞–º—ã–π –≤–µ—Ä–æ—è—Ç–Ω—ã–π –≤ Bee Swarm)
    pcall(function()
        if game:GetService("ReplicatedStorage"):FindFirstChild("Events") then
            local events = game:GetService("ReplicatedStorage").Events
            
            -- –ü–æ–ø—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ —Å–æ–±—ã—Ç–∏—è
            local possibleEvents = {"PlayerClick", "Click", "Collect", "Farm", "Gather"}
            
            for _, eventName in ipairs(possibleEvents) do
                local event = events:FindFirstChild(eventName)
                if event and event:IsA("RemoteEvent") then
                    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∫–ª–∏–∫ –Ω–∞ —Ç–µ–∫—É—â—É—é –ø–æ–∑–∏—Ü–∏—é
                    local character = Player.Character
                    if character and character:FindFirstChild("HumanoidRootPart") then
                        event:FireServer(character.HumanoidRootPart.Position, "Collect")
                        return true
                    end
                end
            end
        end
    end)
    
    -- –ú–µ—Ç–æ–¥ 2: –ï—Å–ª–∏ –µ—Å—Ç—å –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç —Å–±–æ—Ä–∞
    pcall(function()
        local character = Player.Character
        if character then
            local tool = character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Click") then
                tool.Click:Fire()
                return true
            end
        end
    end)
    
    return false
end

function CollectToken(token)
    -- –ü—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –º–µ—Ç–æ–¥—ã —Å–±–æ—Ä–∞ —Ç–æ–∫–µ–Ω–∞
    
    -- –ú–µ—Ç–æ–¥ 1: TouchInterest (—Å–∞–º—ã–π –Ω–∞–¥—ë–∂–Ω—ã–π)
    pcall(function()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local root = character.HumanoidRootPart
            
            -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ —Ç–æ–∫–µ–Ω—É
            root.CFrame = CFrame.new(token.Position + Vector3.new(0, 3, 0))
            
            -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º TouchInterest
            firetouchinterest(root, token, 0)
            firetouchinterest(root, token, 1)
            
            wait(0.1)
            return true
        end
    end)
    
    -- –ú–µ—Ç–æ–¥ 2: ProximityPrompt
    pcall(function()
        if token:FindFirstChildWhichIsA("ProximityPrompt") then
            fireproximityprompt(token:FindFirstChildWhichIsA("ProximityPrompt"))
            return true
        end
    end)
    
    -- –ú–µ—Ç–æ–¥ 3: RemoteEvent
    pcall(function()
        if game:GetService("ReplicatedStorage"):FindFirstChild("Events") then
            local events = game:GetService("ReplicatedStorage").Events
            
            for _, event in pairs(events:GetChildren()) do
                if event:IsA("RemoteEvent") and event.Name:find("Collect") then
                    event:FireServer(token.Name, token.Position)
                    return true
                end
            end
        end
    end)
    
    return false
end

function GetFieldPosition()
    -- –ü—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ –ø–æ–ª–µ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏
    local field = workspace:FindFirstChild(Settings.CurrentField)
    if field and field:IsA("BasePart") then
        return field.Position
    end
    
    -- –ò—â–µ–º –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é –≤ –ø–æ—Ç–æ–º–∫–∞—Ö
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name:find(Settings.CurrentField) and obj:IsA("BasePart") then
            return obj.Position
        end
    end
    
    -- –ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é –¥–ª—è –ø–æ–ø—É–ª—è—Ä–Ω—ã—Ö –ø–æ–ª–µ–π
    local defaultPositions = {
        ["Sunflower Field"] = Vector3.new(0, 0, 0),
        ["Pineapple Patch"] = Vector3.new(100, 0, 0),
        ["Bamboo Field"] = Vector3.new(-100, 0, 0),
        ["Mushroom Field"] = Vector3.new(0, 0, 100),
        ["Clover Field"] = Vector3.new(0, 0, -100)
    }
    
    return defaultPositions[Settings.CurrentField] or Vector3.new(0, 0, 0)
end

function TeleportToField(fieldName)
    local position = GetFieldPosition()
    local character = Player.Character
    
    if character and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = CFrame.new(position + Vector3.new(0, 5, 0))
        print("[–¢–ï–õ–ï–ü–û–†–¢] –ü–µ—Ä–µ–º–µ—â—ë–Ω –Ω–∞ –ø–æ–ª–µ: " .. fieldName)
    end
end

function TeleportToHive()
    pcall(function()
        local hive = workspace.Hives:FindFirstChild(Player.Name)
        if hive then
            Player.Character:SetPrimaryPartCFrame(hive.CFrame + Vector3.new(0, 5, 0))
            print("[–¢–ï–õ–ï–ü–û–†–¢] –ü–µ—Ä–µ–º–µ—â—ë–Ω –∫ —É–ª—å—é")
        end
    end)
end

function ApplySpeedHack()
    pcall(function()
        local character = Player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = Settings.WalkSpeed
                humanoid.JumpPower = Settings.JumpPower
            end
        end
    end)
end

function ShowStats()
    pcall(function()
        local statsText = "\nüìä –°–¢–ê–¢–ò–°–¢–ò–ö–ê –ò–ì–†–û–ö–ê:\n"
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ PlayerStats
        if Player:FindFirstChild("PlayerStats") then
            local playerStats = Player.PlayerStats
            
            if playerStats:FindFirstChild("Pollen") then
                statsText = statsText .. "üåº –ü—ã–ª—å—Ü–∞: " .. playerStats.Pollen.Value .. "\n"
                pollenLabel.Text = "–ü—ã–ª—å—Ü–∞: " .. playerStats.Pollen.Value
            end
            
            if playerStats:FindFirstChild("Honey") then
                statsText = statsText .. "üçØ –ú—ë–¥: " .. playerStats.Honey.Value .. "\n"
            end
        end
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —É–ª–µ–π
        local hive = workspace.Hives:FindFirstChild(Player.Name)
        if hive and hive:FindFirstChild("Bees") then
            local beeCount = #hive.Bees:GetChildren()
            statsText = statsText .. "üêù –ü—á—ë–ª –≤ —É–ª—å–µ: " .. beeCount .. "\n"
            beesLabel.Text = "–ü—á—ë–ª—ã: " .. beeCount
        end
        
        print(statsText)
    end)
end

function OptimizeGame()
    pcall(function()
        settings().Rendering.QualityLevel = 1
        
        for _, effect in pairs(workspace:GetDescendants()) do
            if effect:IsA("ParticleEmitter") then
                effect.Enabled = false
            end
        end
        
        print("[–û–ü–¢–ò–ú–ò–ó–ê–¶–ò–Ø] –ì—Ä–∞—Ñ–∏–∫–∞ —É–ø—Ä–æ—â–µ–Ω–∞ –¥–ª—è FPS")
    end)
end

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ SpeedHack
Player.CharacterAdded:Connect(function()
    wait(2)
    ApplySpeedHack()
end)

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
spawn(function()
    while wait(10) do
        if ScreenGui.Parent then
            ShowStats()
        end
    end
end)

