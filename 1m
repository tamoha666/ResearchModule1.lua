    Settings.AutoFarm = state
    if state then
        print("[–ê–í–¢–û–§–ê–†–ú] –ó–∞–ø—É—â–µ–Ω —Å–±–æ—Ä –ø—ã–ª—å—Ü—ã")
        StartAutoFarm()
    else
        print("[–ê–í–¢–û–§–ê–†–ú] –°–±–æ—Ä –ø—ã–ª—å—Ü—ã –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)
